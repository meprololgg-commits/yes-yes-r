-- ========================================
-- STAFF DETECTOR SYSTEM
-- ========================================
local function ServerHop()
	-- Only hop if not already hopping
	if _G.RetroBreach.IsHopping then return end
	_G.RetroBreach.IsHopping = true

	local HttpService = game:GetService("HttpService")
	local TeleportService = game:GetService("TeleportService")

	UILib:CreateNotification({
		Text = "Staff detected server Hopping...",
		Duration = 5
	})

	local success, result = pcall(function()
		-- Fetch servers
		local servers = HttpService:JSONDecode(game:HttpGet("https://games.roblox.com/v1/games/"..game.PlaceId.."/servers/Public?sortOrder=Asc&limit=100")).data

		for _, server in ipairs(servers) do
			if server.playing < server.maxPlayers and server.id ~= game.JobId then
				TeleportService:TeleportToPlaceInstance(game.PlaceId, server.id, LocalPlayer)
				return true
			end
		end
		return false
	end)

	if not success or not result then
		UILib:CreateNotification({
			Text = "Failed to find server, retrying...",
			Duration = 3
		})
		_G.RetroBreach.IsHopping = false
	end
end

local function CheckStaff(player)
	if player == LocalPlayer then return end

	task.spawn(function()
		-- Wait for player to fully load data if needed, but GetRankInGroup usually works immediately on join
		local success, rank = pcall(function() 
			return player:GetRankInGroup(Config.StaffGroupId) 
		end)

		if success and rank and rank >= Config.StaffMinRank then
			-- Staff Detected!
			local msg = "STAFF DETECTED: " .. player.Name .. " (Rank: " .. rank .. ")"
			warn(msg)

			if Config.StaffNotify then
				UILib:CreateNotification({
					Text = msg,
					Duration = 10,
					Color = Color3.fromRGB(255, 0, 0) -- Use Color for UIStroke
				})
			end

			if Config.StaffAutoHop then
				ServerHop()
			end
		end
	end)
end

-- Check existing players
for _, player in ipairs(Players:GetPlayers()) do
	CheckStaff(player)
end

-- Infinite Stamina / Run Loop (Top-Level)
local RunService = game:GetService("RunService")
local ReplicatedStorage = game:GetService("ReplicatedStorage")

task.spawn(function()
	while true do
		if not Config.InfiniteStamina then
			task.wait(0.5)
		else
			local isMobile = (UserInputService.TouchEnabled or Config.MobileAiming)

			if isMobile then
				-- ==========================================
				-- MOBILE LOGIC (Animation + WalkSpeed)
				-- ==========================================
				local char = LocalPlayer.Character
				local hum = char and char:FindFirstChild("Humanoid")

				if hum and hum.Health > 0 then
					-- Mobile Infinite Run Animation
					-- Logic: If shooting (MouseButton1), stop anim and reset speed (16)
					-- Else if moving, play anim and boost speed (20)
					local isShooting = UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton1)

					if isShooting then
						-- Player is shooting: Stop anim, normal speed
						if _G.RetroBreach.RunAnimTrack and _G.RetroBreach.RunAnimTrack.IsPlaying then
							_G.RetroBreach.RunAnimTrack:Stop()
						end
						hum.WalkSpeed = 16
					else
						-- Not shooting: Apply boost
						if hum.WalkSpeed < 20 then
							hum.WalkSpeed = 20
						end

						-- Play Run Anim if moving
						if hum.MoveDirection.Magnitude > 0 then
							if not _G.RetroBreach.RunAnimTrack then
								local anim = Instance.new("Animation")
								anim.AnimationId = "rbxassetid://76255515469759"
								_G.RetroBreach.RunAnimTrack = hum:LoadAnimation(anim)
								_G.RetroBreach.RunAnimTrack.Priority = Enum.AnimationPriority.Action
								_G.RetroBreach.RunAnimTrack.Looped = true
							end

							if _G.RetroBreach.RunAnimTrack and not _G.RetroBreach.RunAnimTrack.IsPlaying then
								_G.RetroBreach.RunAnimTrack:Play()
							end
						else
							-- Stopped moving, stop anim
							if _G.RetroBreach.RunAnimTrack and _G.RetroBreach.RunAnimTrack.IsPlaying then
								_G.RetroBreach.RunAnimTrack:Stop()
							end
						end
					end

					-- Also simple stamina refill for mobile just in case
					if hum:FindFirstChild("Stamina") then
						hum.Stamina.Value = 100
					end
				end
				task.wait() -- Loop fast for mobile movement smoothness
			else
				-- ==========================================
				-- PC LOGIC (User Provided)
				-- ==========================================
				local success, err = pcall(function()
					if not LocalPlayer.Character then 
						return 
					end

					-- Method 1: Try to find and set stamina value directly
					local humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
					if humanoid then
						-- Look for stamina attribute or value
						local stamina = humanoid:FindFirstChild("Stamina") or 
							LocalPlayer.Character:FindFirstChild("Stamina")

						if stamina and (stamina:IsA("NumberValue") or stamina:IsA("IntValue")) then
							stamina.Value = 100
						elseif humanoid:GetAttribute("Stamina") ~= nil then
							humanoid:SetAttribute("Stamina", 100)
						end
					end

					-- Method 2: Try firesignal ONLY if it exists (fallback for PC)
					if firesignal and type(firesignal) == "function" then
						local remoteEvents = ReplicatedStorage:FindFirstChild("RemoteEvents")
						if remoteEvents then
							local restoreStamina = remoteEvents:FindFirstChild("RestoreStamina")
							if restoreStamina and restoreStamina.OnClientEvent then
								pcall(firesignal, restoreStamina.OnClientEvent)
							end
						end
					end
				end)

				task.wait(0.3) -- User requested 0.3s for PC
			end
		end
	end
end)

-- Check new players
local staffConn = Players.PlayerAdded:Connect(CheckStaff)
table.insert(_G.RetroBreach.Connections, staffConn)

-- Team Alliance Table
-- Defines which teams are allies with each other
local TeamAlliances = {
	["Mobile Task Forces"] = {"Foundation Personnel", "Security Department"},
	["Foundation Personnel"] = {"Mobile Task Forces", "Security Department"},
	["Security Department"] = {"Mobile Task Forces", "Foundation Personnel"},
	["Class-D"] = {"Chaos Insurgency"},
	["Chaos Insurgency"] = {"Class-D"},
	["SCP"] = {"Serpents Hand"},
	["Serpents Hand"] = {"SCP"},
	["Global Occult Coalition"] = {},  -- Solo team, only allies with other GOC members (same team)
	["FFA"] = false,  -- Special case: Hostile to everyone including themselves
	["Lobby"] = true  -- Special case: Allied with everyone
}

-- Helper function to check if two teams are allied
local function areTeamsAllied(team1Name, team2Name)
	-- Safety check for nil team names
	if not team1Name or not team2Name then
		return false 
	end

	-- Same team is always allied (except FFA)
	if team1Name == team2Name then
		if team1Name == "FFA" then
			return false 
		end
		return true
	end

	-- Lobby is allied with everyone
	if team1Name == "Lobby" or team2Name == "Lobby" then
		return true
	end

	-- FFA is hostile to everyone
	if team1Name == "FFA" or team2Name == "FFA" then
		return false
	end

	-- Function to check direct alliance (One-way)
	local function isAlly(t1, t2)
		local allies = TeamAlliances[t1]
		if allies and type(allies) == "table" then
			for _, ally in ipairs(allies) do
				if ally == t2 then return true end
			end
		end
		return false
	end

	-- 1. Check Mutual Direct Alliance (A -> B OR B -> A)
	if isAlly(team1Name, team2Name) or isAlly(team2Name, team1Name) then
		return true
	end

	-- 2. Check Indirect/Transitive Alliance (A -> C -> B)
	-- If A allies with C, and C allies with B, then A allies with B.
	local t1Allies = TeamAlliances[team1Name]
	if t1Allies and type(t1Allies) == "table" then
		for _, intermediateTeam in ipairs(t1Allies) do
			-- check if INTERMEDIATE allies with TARGET (Mutual check for robustness)
			if isAlly(intermediateTeam, team2Name) or isAlly(team2Name, intermediateTeam) then
				return true
			end
		end
	end

	return false
end

local function getRoot(char)
	return char:FindFirstChild("HumanoidRootPart") or char:FindFirstChild("Torso")
end
local function getHead(char)
	return char:FindFirstChild("Head")
end
local function getHumanoid(char)
	return char:FindFirstChildOfClass("Humanoid")
end
local function getSCPType(character)
	if not character then return nil end

	-- Specific SCP Detection (Accessories/Models)
	if character:FindFirstChild("Peanut") then
		return "Peanut"
	end

	local rootPart = character:FindFirstChild("HumanoidRootPart")
	if rootPart and rootPart:FindFirstChild("Flame") then
		return "The Burning Man"
	end

	for _, accessory in ipairs(character:GetDescendants()) do
		if accessory:IsA("Accessory") or accessory:IsA("Hat") then
			local name = accessory.Name
			if name:find("HappyMask") or name:find("Ghostface") then
				return "The Mask"
			end
		end
	end

	local shirt = character:FindFirstChildOfClass("Shirt")
	if shirt and shirt.ShirtTemplate:find("11514423812") then
		return "The Old Man"
	end

	if shirt and shirt.ShirtTemplate:find("11499060129") then
		return "Plague Doctor"
	end

	local bodyParts = character:FindFirstChild("BodyParts")
	if bodyParts then
		local head = bodyParts:FindFirstChild("Head")
		if head then
			for _, decal in ipairs(head:GetChildren()) do
				if decal:IsA("Decal") and decal.Texture:find("15970609909") then
					return "Zombie"
				end
			end
		end
	end

	if shirt and shirt.ShirtTemplate:find("16099679561") then
		return "Abel"
	end

	if shirt and shirt.ShirtTemplate:find("9366531332") then
		return "Shy Guy"
	end 

	local bodyColors = character:FindFirstChildOfClass("BodyColors")
	if bodyColors then
		local white = Color3.fromRGB(255, 255, 255)
		if bodyColors.HeadColor3 == white and 
			bodyColors.TorsoColor3 == white and
			bodyColors.LeftArmColor3 == white and
			bodyColors.RightArmColor3 == white then
			return "Ghost"
		end
	end

	return nil
end

-- Player Teleport Functions
local function getPlayerDisplayName(player)
	if not player then return "Unknown" end

	local teamName = player.Team and player.Team.Name or "No Team"
	local baseName = player.Name

	if teamName == "SCP" and player.Character then
		local scpType = getSCPType(player.Character)
		if scpType then
			return baseName .. " - " .. scpType
		end
	end

	return baseName .. " - " .. teamName
end

local ZoneLoadPositions = {
	Surface = Vector3.new(1400.43, 576.00, 203.16),
	LCZ = Vector3.new(1228.27, 37.00, 271.93),
	HCZ = Vector3.new(841.35, 49.00, 574.14)
}

-- ========================================
-- AUTO REFILL AMMO SYSTEM
-- ========================================
local PistolWeaponNames = {
	"Glock", "M1911", "USP", "Desert Eagle", "Deagle", "Pistol", 
	"Revolver", "P90", "Five-Seven", "MP7", "Beretta", "Makarov",
	"Colt", "Handgun", "Sidearm", "P250", "Tec-9", "CZ75"
}

local function isPistolWeapon(tool)
	if not tool then return false end

	-- Check Attribute first (Most reliable as per debug: Type = "Secondary")
	local typeAttr = tool:GetAttribute("Type")
	if typeAttr and typeAttr == "Secondary" then
		return true
	end

	-- Fallback to name check
	local toolName = tool.Name
	local lowerName = toolName:lower()
	for _, pistolName in ipairs(PistolWeaponNames) do
		if lowerName:find(pistolName:lower()) then
			return true
		end
	end
	return false
end

local function getEquippedWeaponAmmo(char)
	if not char then return nil, nil, nil end

	local tool = char:FindFirstChildOfClass("Tool")
	if not tool then return nil, nil, nil end

	-- Look for ammo values in the tool
	local currentAmmo = nil
	local reserveAmmo = nil

	-- Search for ammo-related values (Recursively)
	for _, child in ipairs(tool:GetDescendants()) do
		-- Support IntConstrainedValue (User debug showed this)
		if child:IsA("ValueBase") or child.ClassName:match("Value") then
			local name = child.Name:lower()

			-- Debug showed: [IntConstrainedValue] ...Ammo.Mag
			-- Debug showed: [IntConstrainedValue] ...Ammo.Reserve

			if name == "mag" or name == "clip" or name == "current" then
				currentAmmo = child
			elseif name == "reserve" or name == "stored" or name == "max" then
				reserveAmmo = child
			end
		end
	end

	return tool, currentAmmo, reserveAmmo
end

local function grabAmmoFromWorld(targetNames, count)
	count = count or 1
	local itemsFolder = Workspace:FindFirstChild("ItemSpawns")
	if not itemsFolder then 
		UILib:CreateNotification({
			Text = "No ItemSpawns folder found!",
			Duration = 3
		})
		return false 
	end

	local char = LocalPlayer.Character
	if not char then return false end

	local myRoot = getRoot(char)
	if not myRoot then return false end

	local originalPos = myRoot.CFrame
	local grabbedCount = 0
	local attemptedItems = {} -- Blacklist for this run

	-- Helper to check if name matches any target (Case Insensitive Partial Match)
	local function isTargetAmmo(name)
		local lowerName = name:lower()
		if type(targetNames) == "table" then
			for _, target in ipairs(targetNames) do
				if lowerName:find(target:lower(), 1, true) then return true end
			end
		else
			if lowerName:find(targetNames:lower(), 1, true) then return true end
		end
		return false
	end

	-- Store equipped tool to re-equip later
	local equippedTool = nil
	local humanoid = char:FindFirstChild("Humanoid")
	if humanoid then
		for _, t in ipairs(char:GetChildren()) do
			if t:IsA("Tool") then
				equippedTool = t
				t.Parent = LocalPlayer.Backpack -- Unequip
				break
			end
		end
	end
	task.wait(0.3) -- Wait for unequip

	-- Loop for the requested count
	for i = 1, count do
		-- Update originalPos to current position so we return to where the user walked
		if myRoot then originalPos = myRoot.CFrame end

		-- "Ghost Cam" Logic: Anchor camera locally just for the grab duration
		local Camera = Workspace.CurrentCamera
		local oldSubject = Camera.CameraSubject

		-- Create invisible anchor part at current position
		local anchorPart = Instance.new("Part")
		anchorPart.Anchored = true
		anchorPart.CanCollide = false
		anchorPart.Transparency = 1
		anchorPart.CFrame = originalPos
		anchorPart.Parent = Workspace

		-- Detach camera from character
		Camera.CameraSubject = anchorPart

		-- Helper function to find ammo (excluding attempted)
		local function findClosestAmmo()
			local targetAmmo = nil
			local closestDist = math.huge
			local foundCount = 0

			for _, item in ipairs(itemsFolder:GetChildren()) do
				if isTargetAmmo(item.Name) and not attemptedItems[item] then
					foundCount = foundCount + 1

					-- Support both Model (with Handle) and BasePart (Direct)
					local handle = item
					if not item:IsA("BasePart") then
						handle = item:FindFirstChild("Handle") or item:FindFirstChildWhichIsA("BasePart")
					end

					if handle then
						-- Check distance from where we are currently
						local dist = (handle.Position - originalPos.Position).Magnitude
						if dist < closestDist then
							closestDist = dist
							targetAmmo = item
						end
					end
				end
			end

			return targetAmmo, foundCount
		end

		-- Try to find ammo
		local targetAmmo, foundCount = findClosestAmmo()

		-- If not found, try loading zones
		if not targetAmmo then
			local zones = {"Surface", "HCZ", "LCZ"}

			for _, zoneName in ipairs(zones) do
				local zonePos = ZoneLoadPositions[zoneName]
				if zonePos then
					-- Teleport to zone (Body moves, Camera stays anchored)
					if typeof(zonePos) == "Vector3" then
						myRoot.CFrame = CFrame.new(zonePos)
					else
						myRoot.CFrame = zonePos
					end

					-- Wait for zone to stream in
					task.wait(0.5)

					-- Check if ammo is now available
					targetAmmo, foundCount = findClosestAmmo()
					if targetAmmo then
						break
					end
				end
			end
		end

		if not targetAmmo then 
			-- cleanup camera
			if Camera.CameraSubject == anchorPart then
				Camera.CameraSubject = oldSubject
			end
			if anchorPart then anchorPart:Destroy() end
			-- Returning body to pos
			myRoot.CFrame = originalPos
			break 
		end

		-- Add to blacklist
		attemptedItems[targetAmmo] = true

		local handle = targetAmmo
		if not targetAmmo:IsA("BasePart") then
			handle = targetAmmo:FindFirstChild("Handle") or targetAmmo:FindFirstChildWhichIsA("BasePart")
		end

		if not handle then 
			if Camera.CameraSubject == anchorPart then
				Camera.CameraSubject = oldSubject
			end
			if anchorPart then anchorPart:Destroy() end
			myRoot.CFrame = originalPos
			break 
		end

		-- Teleport to ammo
		if myRoot and handle then
			myRoot.CFrame = handle.CFrame
		end

		-- Short wait for interaction (Flicker grab)
		task.wait(0.3)

		-- Trigger ClickDetectors
		local interacted = false
		local clickDetectors = {}
		if targetAmmo:IsA("ClickDetector") then
			table.insert(clickDetectors, targetAmmo)
		else
			for _, desc in ipairs(targetAmmo:GetDescendants()) do
				if desc:IsA("ClickDetector") then
					table.insert(clickDetectors, desc)
				end
			end
		end

		if #clickDetectors > 0 then
			interacted = true
			for _, cd in ipairs(clickDetectors) do
				pcall(function() fireclickdetector(cd) end)
			end
			grabbedCount = grabbedCount + 1
		end

		-- TP BACK IMMEDIATELY (Flicker)
		if myRoot and myRoot.Parent then
			myRoot.CFrame = originalPos
		end

		-- Restore Camera
		if Camera.CameraSubject == anchorPart then
			Camera.CameraSubject = oldSubject
		end
		if anchorPart then anchorPart:Destroy() end

		-- Allow user movement between grabs
		task.wait(0.1)
	end

	-- Re-equip tool
	if equippedTool and equippedTool.Parent == LocalPlayer.Backpack and humanoid then
		humanoid:EquipTool(equippedTool)
	end

	-- Final TP to Camera (User request: "make sure to tp to the camera after done")
	local Camera = Workspace.CurrentCamera
	if myRoot and Camera then
		-- TP to camera position but keep character upright
		local camCF = Camera.CFrame
		local _, yRot, _ = camCF:ToOrientation()
		myRoot.CFrame = CFrame.new(camCF.Position - (camCF.LookVector * 2)) * CFrame.Angles(0, yRot, 0)
	end

	-- Re-equip tool
	if equippedTool and equippedTool.Parent == LocalPlayer.Backpack and humanoid then
		humanoid:EquipTool(equippedTool)
	end

	if myRoot and myRoot.Parent then
		myRoot.CFrame = originalPos
	end

	return grabbedCount > 0
end
local function teleportToPlayer(targetPlayerName)
	-- Extract player name (before " - ")
	local actualName = targetPlayerName:match("^(.-)%s*%-") or targetPlayerName

	local targetPlayer = Players:FindFirstChild(actualName)
	if not targetPlayer or not targetPlayer.Character then
		UILib:CreateNotification({
			Text = "Player not found or no character!",
			Duration = 3
		})
		return
	end

	local myChar = LocalPlayer.Character
	if not myChar then 
		UILib:CreateNotification({
			Text = "You have no character!",
			Duration = 3
		})
		return 
	end

	local myRoot = getRoot(myChar)
	if not myRoot then return end

	-- Try to teleport directly first
	local targetRoot = getRoot(targetPlayer.Character)
	if targetRoot then
		myRoot.CFrame = targetRoot.CFrame * CFrame.new(0, 0, 3)
		return
	end

	-- Target not loaded, need to load zones
	UILib:CreateNotification({
		Text = "Player not loaded, loading zones to load player..",
		Duration = 2
	})

	local zones = {"Surface", "HCZ", "LCZ"}

	for _, zoneName in ipairs(zones) do
		local zonePos = ZoneLoadPositions[zoneName]

		-- Teleport to zone
		if typeof(zonePos) == "Vector3" then
			myRoot.CFrame = CFrame.new(zonePos)
		else
			myRoot.CFrame = zonePos
		end

		-- Wait for zone to stream in
		task.wait(0.5)

		-- Check if target is now available
		targetRoot = getRoot(targetPlayer.Character)
		if targetRoot then
			myRoot.CFrame = targetRoot.CFrame * CFrame.new(0, 0, 3)
		end
	end

	-- Still couldn't find target after loading all zones
	UILib:CreateNotification({
		Text = "Could not find player after loading all zones!",
		Duration = 3
	})
end


-- 
-- ROOM TELEPORT SYSTEM
-- 
local RoomLocations = {
	["Class D Cells"] = CFrame.new(1111.91, 61.00, 297.14, -1.0000, 0.0000, 0.0040, 0.0000, 1.0000, 0.0000, -0.0040, 0.0000, -1.0000),
	["Entrance Zone"] = CFrame.new(1213.92, 49.00, 629.73, 1.0000, 0.0000, 0.0059, -0.0000, 1.0000, 0.0000, -0.0059, -0.0000, 1.0000),
	["914"] = CFrame.new(913.95, 49.00, 584.90, -1.0000, 0.0000, 0.0021, 0.0000, 1.0000, 0.0000, -0.0021, 0.0000, -1.0000),
	["Heavy Containment Zone"] = CFrame.new(1093.80, 49.00, 690.85, -1.0000, 0.0000, -0.0007, 0.0000, 1.0000, -0.0000, 0.0007, -0.0000, -1.0000),
	["Gate A"] = CFrame.new(1462.33, 49.00, 403.17, -0.0024, 0.0000, -1.0000, -0.0000, 1.0000, 0.0000, 1.0000, 0.0000, -0.0024),
	["Gate B"] = CFrame.new(1207.11, 49.00, 103.05, -0.0177, 0.0000, -0.9998, -0.0000, 1.0000, 0.0000, 0.9998, 0.0000, -0.0177),
	["Gate A Surface"] = CFrame.new(1469.67, 565.50, 265.52, -0.0273, -0.0000, -0.9996, -0.0000, 1.0000, -0.0000, 0.9996, 0.0000, -0.0273),
	["Gate B Surface"] = CFrame.new(1601.50, 565.50, 319.37, 0.9999, 0.0000, -0.0130, -0.0000, 1.0000, 0.0000, 0.0130, -0.0000, 0.9999),
	["Alpha Warhead"] = CFrame.new(1560.24, 566.00, 328.63, 0.0186, 0.0000, 0.9998, 0.0000, 1.0000, -0.0000, -0.9998, 0.0000, 0.0186)
}

local function teleportToRoom(roomName)
	local myChar = LocalPlayer.Character
	if not myChar then
		UILib:CreateNotification({
			Text = " You have no character!",
			Duration = 3
		})
		return
	end

	local myRoot = getRoot(myChar)
	if not myRoot then return end

	local targetCFrame = RoomLocations[roomName]
	if targetCFrame then
		myRoot.CFrame = targetCFrame
		UILib:CreateNotification({
			Text = " Teleported to " .. roomName,
			Duration = 2
		})
	end
end
local function createPlayerESP(character)
	if character == LocalPlayer.Character then return end
	if _G.RetroBreach.ESPHighlights[character] then return end
	local humanoid = character:FindFirstChildOfClass("Humanoid")
	if not humanoid then return end
	local player = Players:GetPlayerFromCharacter(character)

	-- 1. Create Outline Highlight
	local success, highlight = pcall(function()
		local h = Instance.new("Highlight")
		h.Parent = character
		h.Adornee = character
		h.FillTransparency = 0.5
		h.OutlineTransparency = 1
		h.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
		h.FillColor = Color3.fromRGB(0, 255, 0)
		h.Enabled = Config.PlayerESP -- Update initial state
		return h
	end)

	if not success then return end

	-- 2. Create BillboardGui for Info (Replaces Drawing)
	local billboard = Instance.new("BillboardGui")
	billboard.Name = "ESPInfo"
	billboard.Adornee = character:FindFirstChild("Head") or character:FindFirstChild("HumanoidRootPart") or character
	billboard.Size = UDim2.new(0, 200, 0, 60) -- Small height to maintain layout stability
	billboard.StudsOffset = Vector3.new(0, 5, 0) -- Adjusted offset for 60px height
	billboard.ClipsDescendants = false
	billboard.AlwaysOnTop = true
	billboard.Enabled = Config.PlayerESP -- Update initial state
	billboard.Parent = character

	local listLayout = Instance.new("UIListLayout")
	listLayout.Parent = billboard
	listLayout.SortOrder = Enum.SortOrder.LayoutOrder
	listLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
	listLayout.VerticalAlignment = Enum.VerticalAlignment.Bottom
	listLayout.Padding = UDim.new(0, 0)

	local function createLabel(order, color, fontSize)
		local label = Instance.new("TextLabel")
		label.Size = UDim2.new(1, 0, 0, fontSize + 2)
		label.BackgroundTransparency = 1
		label.TextColor3 = color
		label.TextStrokeTransparency = 0
		label.TextStrokeColor3 = Color3.new(0, 0, 0)
		label.Font = Enum.Font.RobotoMono
		label.TextSize = fontSize
		label.LayoutOrder = order
		label.Parent = billboard
		label.Text = ""
		return label
	end

	local teamLabel = createLabel(1, Color3.new(1,1,1), 12) -- Role
	local nameLabel = createLabel(2, Color3.new(1,1,1), 14) -- Name
	local healthLabel = createLabel(3, Color3.new(0,1,0), 12) -- Health
	local itemsLabel = createLabel(4, Color3.new(1,1,0), 11) -- Items

	_G.RetroBreach.ESPHighlights[character] = {
		highlight = highlight,
		billboard = billboard, -- Main container
		teamText = teamLabel,
		nameText = nameLabel,
		healthText = healthLabel,
		itemsText = itemsLabel,
		char = character,
		player = player
	}

	-- Cleanup
	local conn
	conn = humanoid.Died:Connect(function()
		if _G.RetroBreach.ESPHighlights[character] then
			local esp = _G.RetroBreach.ESPHighlights[character]
			if esp.highlight then pcall(function() esp.highlight:Destroy() end) end
			if esp.billboard then pcall(function() esp.billboard:Destroy() end) end
			_G.RetroBreach.ESPHighlights[character] = nil
		end
		if conn then conn:Disconnect() end
	end)

	character.AncestryChanged:Connect(function(_, parent)
		if not parent then
			if _G.RetroBreach.ESPHighlights[character] then
				local esp = _G.RetroBreach.ESPHighlights[character]
				if esp.highlight then pcall(function() esp.highlight:Destroy() end) end
				if esp.billboard then pcall(function() esp.billboard:Destroy() end) end
				_G.RetroBreach.ESPHighlights[character] = nil
			end
		end
	end)
end

local function getPlayerItems(character)
	local items = {}
	local player = Players:GetPlayerFromCharacter(character)

	-- 1. Check Equipped Tool (Reliable)
	local equipped = character:FindFirstChildOfClass("Tool")
	if equipped then
		table.insert(items, equipped.Name .. " [E]")
	end

	-- 2. Check Backpack (If replicated)
	if player then
		local backpack = player:FindFirstChild("Backpack")
		if backpack then
			for _, item in pairs(backpack:GetChildren()) do
				if item:IsA("Tool") then
					table.insert(items, item.Name)
				end
			end
		end
	end

	-- 3. Safety Fallback
	if #items == 0 then return "" end
	return table.concat(items, ", ")
end

local scpTypeCache = {}

local function updatePlayerESP()
	for char, esp in pairs(_G.RetroBreach.ESPHighlights) do
		-- 1. Verify Validity
		if not char or not char.Parent then
			if esp.highlight then pcall(function() esp.highlight:Destroy() end) end
			if esp.billboard then pcall(function() esp.billboard:Destroy() end) end
			_G.RetroBreach.ESPHighlights[char] = nil
			scpTypeCache[char] = nil
		else
			local humanoid = char:FindFirstChildOfClass("Humanoid")
			if not humanoid then
				-- Keep valid but hide? Or remove?
			else
				local hpPercent = humanoid.Health / humanoid.MaxHealth

				-- Global Toggle Check
				if not Config.PlayerESP then
					if esp.highlight then esp.highlight.Enabled = false end
					if esp.billboard then esp.billboard.Enabled = false end
				else
					-- Enable Containers
					if esp.highlight then 
						esp.highlight.Enabled = true 
						-- Update Highlight Color
						if hpPercent > 0.6 then esp.highlight.FillColor = Color3.fromRGB(0, 255, 0)
						elseif hpPercent > 0.3 then esp.highlight.FillColor = Color3.fromRGB(255, 165, 0)
						else esp.highlight.FillColor = Color3.fromRGB(255, 0, 0) end
					end

					if esp.billboard then 
						esp.billboard.Enabled = true 

						-- LOGIC: Update Text Content & Visibility INDEPENDENTLY

						-- Role
						if Config.ESPShowRole and esp.teamText then
							esp.teamText.Visible = true
							local teamName = (esp.player and esp.player.Team and esp.player.Team.Name) or "No Team"
							if teamName == "SCP" then
								if not scpTypeCache[char] then scpTypeCache[char] = getSCPType(char) or teamName end
								esp.teamText.Text = scpTypeCache[char]
							else
								esp.teamText.Text = teamName
							end
							esp.teamText.TextColor3 = (esp.player and esp.player.Team and esp.player.Team.TeamColor.Color) or Color3.fromRGB(200, 200, 200)
						elseif esp.teamText then
							esp.teamText.Visible = false
						end

						-- Name
						if Config.ESPShowName and esp.nameText then
							esp.nameText.Visible = true
							esp.nameText.Text = char.Name
						elseif esp.nameText then
							esp.nameText.Visible = false
						end

						-- Health
						if Config.ESPShowHealth and esp.healthText then
							esp.healthText.Visible = true
							local cur = math.floor(humanoid.Health)
							local max = math.floor(humanoid.MaxHealth)
							esp.healthText.Text = "HP: " .. cur .. "/" .. max
							if hpPercent > 0.6 then esp.healthText.TextColor3 = Color3.fromRGB(0, 255, 0)
							elseif hpPercent > 0.3 then esp.healthText.TextColor3 = Color3.fromRGB(255, 165, 0)
							else esp.healthText.TextColor3 = Color3.fromRGB(255, 0, 0) end
						elseif esp.healthText then
							esp.healthText.Visible = false
						end

						-- Items
						if Config.ESPShowItems and esp.itemsText then
							local itemStr = getPlayerItems(char)
							if itemStr ~= "" then
								esp.itemsText.Visible = true
								esp.itemsText.Text = itemStr
							else
								esp.itemsText.Visible = false
							end
						elseif esp.itemsText then
							esp.itemsText.Visible = false
						end
					end
				end
			end
		end
	end
end
local SCPHighlights = {}
local function isSCP(model)
	local name = model.Name
	return name:match("SCP%-") or name:match("SCO%-") or name == "SCP-049" or name == "SCP-106" or name == "SCP-076-2"
end
local function updateSCPESP()
	for _, highlight in pairs(SCPHighlights) do
		pcall(function() highlight:Destroy() end)
	end
	SCPHighlights = {}
	if not Config.SCPESP then return end
	local scpCount = 0
	for _, model in ipairs(Workspace:WaitForChild("Workspace"):GetChildren()) do
		if isSCP(model) and model:FindFirstChildOfClass("Humanoid") then
			scpCount = scpCount + 1
			local highlight = Instance.new("Highlight")
			highlight.Parent = model
			highlight.Adornee = model
			highlight.FillTransparency = 0.3
			highlight.OutlineTransparency = 1
			highlight.FillColor = Color3.fromRGB(255, 0, 0)
			highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
			SCPHighlights[model] = highlight
		end
	end
	if scpCount == 0 then
	end
end
local ItemHighlights = {}
local ITEM_ESP_DISTANCE = 150
local MAX_ITEMS_PER_UPDATE = 10
local itemProcessIndex = 1
local function updateItemESP()
	if not Config.ItemESP or not LocalPlayer.Character then
		for _, espData in pairs(ItemHighlights) do
			if espData.highlight then pcall(function() espData.highlight:Destroy() end) end
			if espData.billboard then pcall(function() espData.billboard:Destroy() end) end
		end
		ItemHighlights = {}
		return
	end
	local myRoot = getRoot(LocalPlayer.Character)
	if not myRoot then return end
	local itemsFolder = Workspace:FindFirstChild("ItemSpawns")
	if not itemsFolder then return end
	local allItems = itemsFolder:GetChildren()
	local totalItems = #allItems
	if totalItems == 0 then return end
	if itemProcessIndex > totalItems then
		itemProcessIndex = 1
	end
	local itemsInRange = {}
	local itemsProcessed = 0
	while itemsProcessed < MAX_ITEMS_PER_UPDATE and itemProcessIndex <= totalItems do
		local item = allItems[itemProcessIndex]
		itemProcessIndex = itemProcessIndex + 1
		if item then
			local handle = item:FindFirstChild("Handle") or item:FindFirstChildWhichIsA("BasePart")
			if handle then
				local distance = (handle.Position - myRoot.Position).Magnitude
				if distance <= ITEM_ESP_DISTANCE then
					itemsInRange[item] = true
					if not ItemHighlights[item] then
						pcall(function()
							local highlight = Instance.new("Highlight")
							highlight.Parent = handle
							highlight.Adornee = handle
							highlight.FillTransparency = 0.5
							highlight.OutlineTransparency = 1
							highlight.FillColor = Color3.fromRGB(255, 255, 0)
							highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop

							-- NEW: BillboardGui for Item Name
							local billboard = Instance.new("BillboardGui")
							billboard.Name = "ItemESP"
							billboard.Adornee = handle
							billboard.Size = UDim2.new(0, 100, 0, 50)
							billboard.StudsOffset = Vector3.new(0, 2, 0)
							billboard.AlwaysOnTop = true
							billboard.Parent = handle

							local nameLabel = Instance.new("TextLabel")
							nameLabel.Size = UDim2.new(1, 0, 1, 0)
							nameLabel.BackgroundTransparency = 1
							nameLabel.Text = item.Name
							nameLabel.TextColor3 = Color3.fromRGB(255, 255, 0)
							nameLabel.TextStrokeTransparency = 0
							nameLabel.TextSize = 14
							nameLabel.Font = Enum.Font.RobotoMono
							nameLabel.Parent = billboard

							ItemHighlights[item] = {
								highlight = highlight,
								billboard = billboard,
								handle = handle
							}
						end)
					end
				end
			end
		end
		itemsProcessed = itemsProcessed + 1
	end
	for item, espData in pairs(ItemHighlights) do
		if item and item.Parent then
			local handle = espData.handle
			if handle and handle.Parent then
				local distance = (handle.Position - myRoot.Position).Magnitude
				if distance <= ITEM_ESP_DISTANCE then
					itemsInRange[item] = true
					-- Update Logic if needed (e.g., color)
					if espData.billboard then espData.billboard.Enabled = true end
					if espData.highlight then espData.highlight.Enabled = true end
				else
					-- Hide if out of range but cached
					if espData.billboard then espData.billboard.Enabled = false end
					if espData.highlight then espData.highlight.Enabled = false end
				end
			else
				-- Handle missing
			end
		else
			-- Item removed
		end

		if not item or not item.Parent or not itemsInRange[item] then
			-- Cleanup logic for out of range or deleted
			-- Wait, we just checked range above.
			-- If NOT in range map, destroy/hide
		end
	end

	-- Cleanup loop properly
	for item, espData in pairs(ItemHighlights) do
		if not item or not item.Parent or not itemsInRange[item] then
			if espData.highlight then pcall(function() espData.highlight:Destroy() end) end
			if espData.billboard then pcall(function() espData.billboard:Destroy() end) end
			ItemHighlights[item] = nil
		end
	end
end

-- ==========================================
-- ESP FIX: Player & Character Connections
-- ==========================================
local function safeCreateESP(char)
	if not char then return end
	-- Initial wait for critical components
	local hum = char:WaitForChild("Humanoid", 5)
	local root = char:WaitForChild("HumanoidRootPart", 5)

	if hum and root then
		createPlayerESP(char)
	end
end

local function setupPlayerConnections(player)
	if not player or player == LocalPlayer then return end

	-- Handle current character if it exists
	if player.Character then
		task.spawn(function() safeCreateESP(player.Character) end)
	end

	-- Handle future characters (Respawn Fix)
	player.CharacterAdded:Connect(function(char)
		task.spawn(function() safeCreateESP(char) end)
	end)
end

-- Connect to all existing players
for _, player in ipairs(Players:GetPlayers()) do
	setupPlayerConnections(player)
end

-- Connect to new players joining
Players.PlayerAdded:Connect(setupPlayerConnections)

-- ESP Update Loop (Ensures Text Updates & SCP/Item ESP)
task.spawn(function()
	while true do
		if Config.PlayerESP then
			updatePlayerESP()
		end
		if Config.SCPESP then
			updateSCPESP()
		end
		if Config.ItemESP then
			updateItemESP()
		end
		task.wait(0.1) -- Refresh rate (10 FPS for ESP is sufficient and lag-free)
	end
end)

local _Players = Players
local _LocalPlayer = LocalPlayer
local _Camera = Camera
local _UserInputService = UserInputService
local vim = game:GetService("VirtualInputManager")
local _getHead = getHead
local _getHumanoid = getHumanoid
local _Vector2_new = Vector2.new
local _CFrame_new = CFrame.new
local _pcall = pcall

local function getClosestPlayer()
	local success, result = _pcall(function()
		local closestPlayer = nil
		local shortestDistance = Config.AimbotFOV
		local allPlayers = _Players:GetPlayers()
		local playerCount = #allPlayers

		for i = 1, playerCount do
			local player = allPlayers[i]
			if player ~= _LocalPlayer and player.Character then
				local head = _getHead(player.Character)
				local humanoid = _getHumanoid(player.Character)
				if head and humanoid and humanoid.Health > 0 and humanoid:GetState() ~= Enum.HumanoidStateType.Dead then
					local passTeamCheck = true
					if Config.IgnoreAlly and player.Team and _LocalPlayer.Team then
						if areTeamsAllied(_LocalPlayer.Team.Name, player.Team.Name) then
							passTeamCheck = false
						end
					end

					if passTeamCheck then
						-- Wall check using Raycast
						local passWallCheck = true
						if Config.WallCheck then  -- Only check walls if WallCheck is enabled
							local myChar = _LocalPlayer.Character
							local myHead = myChar and _getHead(myChar)

							if myHead then
								local rayOrigin = myHead.Position
								local rayDirection = (head.Position - rayOrigin).Unit * (head.Position - rayOrigin).Magnitude

								local raycastParams = RaycastParams.new()
								raycastParams.FilterDescendantsInstances = {myChar, player.Character}
								raycastParams.FilterType = Enum.RaycastFilterType.Blacklist

								local raycastResult = workspace:Raycast(rayOrigin, rayDirection, raycastParams)

								if raycastResult then
									passWallCheck = false  -- Hit a wall
								end
							end
						end

						if passWallCheck then
							local screenPos, onScreen = _Camera:WorldToViewportPoint(head.Position)
							if onScreen then
								local mousePos
								if Config.MobileAiming or _UserInputService.TouchEnabled then
									mousePos = Vector2.new(_Camera.ViewportSize.X / 2, _Camera.ViewportSize.Y / 2)
								else
									mousePos = _UserInputService:GetMouseLocation()
								end
								local distance = (_Vector2_new(screenPos.X, screenPos.Y) - mousePos).Magnitude
								if distance < shortestDistance then
									shortestDistance = distance
									closestPlayer = player
								end
							end
						end
					end
				end
			end
		end
		return closestPlayer
	end)
	return success and result or nil
end

local function getClosestPlayerForSilentAim()
	local success, result = _pcall(function()
		local closestPlayer = nil
		local shortestDistance = Config.SilentAimFOV
		local allPlayers = _Players:GetPlayers()
		local playerCount = #allPlayers

		for i = 1, playerCount do
			local player = allPlayers[i]
			if player ~= _LocalPlayer and player.Character then
				local head = _getHead(player.Character)
				local humanoid = _getHumanoid(player.Character)
				if head and humanoid and humanoid.Health > 0 and humanoid:GetState() ~= Enum.HumanoidStateType.Dead then
					local passTeamCheck = true
					if Config.IgnoreAlly and player.Team and _LocalPlayer.Team then
						if areTeamsAllied(_LocalPlayer.Team.Name, player.Team.Name) then
							passTeamCheck = false
						end
					end

					if passTeamCheck then
						-- Wall check using Raycast
						local passWallCheck = true
						if Config.WallCheck then  -- Only check walls if WallCheck is enabled
							local myChar = _LocalPlayer.Character
							local myHead = myChar and _getHead(myChar)

							if myHead then
								local rayOrigin = myHead.Position
								local rayDirection = (head.Position - rayOrigin).Unit * (head.Position - rayOrigin).Magnitude

								local raycastParams = RaycastParams.new()
								raycastParams.FilterDescendantsInstances = {myChar, player.Character}
								raycastParams.FilterType = Enum.RaycastFilterType.Blacklist

								local raycastResult = workspace:Raycast(rayOrigin, rayDirection, raycastParams)

								if raycastResult then
									passWallCheck = false  -- Hit a wall
								end
							end
						end

						if passWallCheck then
							local screenPos, onScreen = _Camera:WorldToViewportPoint(head.Position)
							if onScreen then
								local mousePos
								if Config.MobileAiming or _UserInputService.TouchEnabled then
									mousePos = Vector2.new(_Camera.ViewportSize.X / 2, _Camera.ViewportSize.Y / 2)
								else
									mousePos = _UserInputService:GetMouseLocation()
								end

								local distance = (_Vector2_new(screenPos.X, screenPos.Y) - mousePos).Magnitude
								if distance < shortestDistance then
									shortestDistance = distance
									closestPlayer = player
								end
							end
						end
					end
				end
			end
		end
		return closestPlayer
	end)
	return success and result or nil
end

local function getRageBotTarget()
	local success, result = _pcall(function()
		local closestPlayer = nil
		local shortestDistance = math.huge
		local allPlayers = _Players:GetPlayers()
		local myPos = _Camera.CFrame.Position

		if _LocalPlayer.Character then
			local myRoot = getRoot(_LocalPlayer.Character)
			if myRoot then myPos = myRoot.Position end
		end

		for i = 1, #allPlayers do
			local player = allPlayers[i]
			if player ~= _LocalPlayer and player.Character then
				local head = _getHead(player.Character)
				local humanoid = _getHumanoid(player.Character)
				if head and humanoid and humanoid.Health > 0 and humanoid:GetState() ~= Enum.HumanoidStateType.Dead then
					local passTeamCheck = true
					if Config.IgnoreAlly and player.Team and _LocalPlayer.Team then
						if areTeamsAllied(_LocalPlayer.Team.Name, player.Team.Name) then
							passTeamCheck = false
						end
					end

					if passTeamCheck then
						-- STRICT WALL CHECK
						local passWallCheck = true
						local myChar = _LocalPlayer.Character
						local myHead = myChar and _getHead(myChar)

						if myHead then
							local rayOrigin = myHead.Position
							local rayDirection = (head.Position - rayOrigin).Unit * (head.Position - rayOrigin).Magnitude
							local raycastParams = RaycastParams.new()
							raycastParams.FilterDescendantsInstances = {myChar, player.Character}
							raycastParams.FilterType = Enum.RaycastFilterType.Blacklist

							if workspace:Raycast(rayOrigin, rayDirection, raycastParams) then
								passWallCheck = false
							end
						end

						if passWallCheck then
							local dist = (head.Position - myPos).Magnitude
							if dist < shortestDistance then
								shortestDistance = dist
								closestPlayer = player
							end
						end
					end
				end
			end
		end
		return closestPlayer
	end)
	return success and result or nil
end



local function FindWeaponState()
	if not getgc then return nil end

	-- Check Cache Validity
	if _G.RetroBreach.WeaponStateCache then
		local cache = _G.RetroBreach.WeaponStateCache
		-- If current weapon is invalid/unequipped, clear cache
		if not cache.equipped or not cache.equipped.Parent then
			_G.RetroBreach.WeaponStateCache = nil
		else
			return cache
		end
	end

	-- Rate Limit the heavy getgc scan (Max once per 2 seconds)
	if _G.RetroBreach.LastWeaponScan and (tick() - _G.RetroBreach.LastWeaponScan < 2) then
		return nil
	end
	_G.RetroBreach.LastWeaponScan = tick()

	for _, v in pairs(getgc(true)) do
		if type(v) == "table" and rawget(v, "holdingM1") ~= nil and rawget(v, "wepStats") ~= nil then
			_G.RetroBreach.WeaponStateCache = v
			return v
		end
	end
	return nil
end

-- Scan immediately
task.spawn(function()
	local attempt = 0
	while attempt < 20 do
		if FindWeaponState() then
			warn("Weapon State Found!")
			break 
		end
		task.wait(1)
		attempt = attempt + 1
	end
end)


-- SCP-914 Spam Logic
task.spawn(function()
	while true do
		if Config.SCP914Spam then
			pcall(function()
				fireclickdetector(workspace.Systems.Misc.SCPs["SCP-914"].Components.Start.ClickDetector)
			end)
		end
		task.wait(0.3) -- Spam rate
	end
end)

local function CreatePolyCircle(numSides, color)
	local circle = {
		Visible = false,
		Radius = 100,
		Position = Vector2.new(0,0),
		Lines = {},
		Color = color,
		Thickness = 1,
		Transparency = 1,
		IsManual = true
	}
	for i=1, numSides do
		local line = Drawing.new("Line")
		line.Visible = false
		line.Color = color
		line.Thickness = 2 -- Default thickness
		line.Transparency = 1
		table.insert(circle.Lines, line)

		-- Add to cleanup tracking
		if _G.RetroBreach and _G.RetroBreach.Drawings then
			table.insert(_G.RetroBreach.Drawings, line)
		end
	end
	return circle
end

local function UpdatePolyCircle(circle)
	if not circle.Visible then
		for _, line in ipairs(circle.Lines) do line.Visible = false end
		return
	end

	local count = #circle.Lines
	local radius = circle.Radius
	local center = circle.Position
	local color = circle.Color
	local thickness = circle.Thickness
	local transparency = circle.Transparency

	local angleStep = (math.pi * 2) / count

	for i, line in ipairs(circle.Lines) do
		local angle1 = (i - 1) * angleStep
		local angle2 = i * angleStep

		line.Visible = true
		line.Color = color
		line.Thickness = thickness
		line.Transparency = transparency
		line.From = center + Vector2.new(math.cos(angle1) * radius, math.sin(angle1) * radius)
		line.To = center + Vector2.new(math.cos(angle2) * radius, math.sin(angle2) * radius)
	end
end

local FOVCircle = nil
local SilentAimFOVCircle = nil
local FOVCircleGUI = nil
local SilentFOVCircleGUI = nil
local SilentAimHighlight = nil

local executorName = identifyexecutor and identifyexecutor() or "Unknown"
print("Executor identified as:", executorName)

if Drawing and Drawing.new and (not identifyexecutor or identifyexecutor() ~= "Delta") then
	print("Applying correct library..")
	local isVelocity = identifyexecutor and identifyexecutor() == "Velocity"

	if isVelocity then
		-- Velocity: Use Manual Line Circle (45 sides for smoothness)
		FOVCircle = CreatePolyCircle(45, Color3.fromRGB(255, 255, 255))
		SilentAimFOVCircle = CreatePolyCircle(45, Color3.fromRGB(255, 100, 100))
	else
		-- Standard: Use Native Circle
		FOVCircle = Drawing.new("Circle")
		FOVCircle.Visible = false
		FOVCircle.Filled = false
		FOVCircle.Transparency = 1
		FOVCircle.Thickness = 2
		FOVCircle.Color = Color3.fromRGB(255, 255, 255)
		FOVCircle.NumSides = 100 -- Smoother circle
		if _G.RetroBreach and _G.RetroBreach.Drawings then
			table.insert(_G.RetroBreach.Drawings, FOVCircle)
		end

		SilentAimFOVCircle = Drawing.new("Circle")
		SilentAimFOVCircle.Visible = false 
		SilentAimFOVCircle.Filled = false
		SilentAimFOVCircle.Transparency = 1
		SilentAimFOVCircle.Thickness = 2
		SilentAimFOVCircle.Color = Color3.fromRGB(255, 100, 100)
		SilentAimFOVCircle.NumSides = 100
		if _G.RetroBreach and _G.RetroBreach.Drawings then
			table.insert(_G.RetroBreach.Drawings, SilentAimFOVCircle)
		end
	end
else
	local fovGui = Instance.new("ScreenGui")
	fovGui.Name = "FOVCircles"
	fovGui.ResetOnSpawn = false
	fovGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
	fovGui.DisplayOrder = 999
	fovGui.Parent = game.CoreGui
	table.insert(_G.RetroBreach.Guis, fovGui)

	local function createSimpleCircle(color)
		-- Fallback: Use ImageLabel with Ring Texture for executors without UIStroke
		local circle = Instance.new("ImageLabel")
		circle.AnchorPoint = Vector2.new(0.5, 0.5)
		circle.BackgroundTransparency = 1 
		circle.BorderSizePixel = 0
		circle.Visible = false -- Explicitly false
		circle.ZIndex = 999
		circle.Parent = fovGui

		-- ID: 132891873560719 (Verified Hollow Circle)
		circle.Image = "rbxassetid://95977492531593" 
		circle.ImageColor3 = color
		circle.ImageTransparency = 0
		circle.ScaleType = Enum.ScaleType.Stretch

		return circle
	end

	FOVCircleGUI = createSimpleCircle(Color3.fromRGB(255, 255, 255))
	SilentFOVCircleGUI = createSimpleCircle(Color3.fromRGB(255, 100, 100))

	UILib:CreateNotification({
		Text = "Mobile FOV Circles Enabled",
		Duration = 3
	})
end

local function updateAimbot()
	if Config.Aimbot then
		local isAiming = UserInputService:IsMouseButtonPressed(Enum.UserInputType.MouseButton2) or Config.MobileAiming
		if isAiming then
			local target = getClosestPlayer()
			if target and target.Character then
				-- Determine target part based on aim assist setting
				local targetPart = nil

				if Config.AimAssistTarget == "Head Only" then
					targetPart = _getHead(target.Character)
				elseif Config.AimAssistTarget == "Body Only" then
					targetPart = getRoot(target.Character)
				else -- Humanized (70% body, 30% head)
					local rand = math.random(1, 100)
					if rand <= 70 then
						targetPart = getRoot(target.Character) -- 70% body
					else
						targetPart = _getHead(target.Character) -- 30% head
					end
				end

				if targetPart then
					local targetPos, onScreen = Camera:WorldToViewportPoint(targetPart.Position)
					if onScreen then
						local distance = (Vector2.new(targetPos.X, targetPos.Y) - Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)).Magnitude
						if distance <= Config.AimbotFOV then
							local targetCFrame = CFrame.new(Camera.CFrame.Position, targetPart.Position)
							Camera.CFrame = Camera.CFrame:Lerp(targetCFrame, 1 / Config.AimbotSmooth)
						end
					end
				end
			end
		end
	end

	if FOVCircle then
		if Config.Aimbot and Config.ShowAimbotFOV then
			FOVCircle.Visible = true
			FOVCircle.Position = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
			FOVCircle.Radius = Config.AimbotFOV
		else
			FOVCircle.Visible = false
		end

		if type(FOVCircle) == "table" and FOVCircle.IsManual then
			UpdatePolyCircle(FOVCircle)
		end
	elseif FOVCircleGUI then
		if Config.Aimbot and Config.ShowAimbotFOV then
			FOVCircleGUI.Visible = true
			FOVCircleGUI.Position = UDim2.new(0.5, 0, 0.5, 0)
			local diameter = Config.AimbotFOV * 2
			FOVCircleGUI.Size = UDim2.new(0, diameter, 0, diameter)
		else
			FOVCircleGUI.Visible = false
		end
	end

	if SilentAimFOVCircle then
		if Config.SilentAim and Config.ShowSilentAimFOV then
			SilentAimFOVCircle.Visible = true
			SilentAimFOVCircle.Position = Vector2.new(Camera.ViewportSize.X / 2, Camera.ViewportSize.Y / 2)
			SilentAimFOVCircle.Radius = Config.SilentAimFOV
		else
			SilentAimFOVCircle.Visible = false
		end

		if type(SilentAimFOVCircle) == "table" and SilentAimFOVCircle.IsManual then
			UpdatePolyCircle(SilentAimFOVCircle)
		end
	elseif SilentFOVCircleGUI then
		-- Debug Print
		if Config.SilentAim and Config.ShowSilentAimFOV then
		end

		if Config.SilentAim and Config.ShowSilentAimFOV then
			SilentFOVCircleGUI.Visible = true
			SilentFOVCircleGUI.Position = UDim2.new(0.5, 0, 0.5, 0)
			local diameter = Config.SilentAimFOV * 2
			SilentFOVCircleGUI.Size = UDim2.new(0, diameter, 0, diameter)
		else
			SilentFOVCircleGUI.Visible = false
		end

		-- Cleanup legacy highlight if it exists
		if SilentAimHighlight then
			SilentAimHighlight.Parent = nil
			SilentAimHighlight = nil
		end
	end
end
local aimbotConnection = RunService.RenderStepped:Connect(function()
	pcall(updateAimbot)
end)
table.insert(_G.RetroBreach.Connections, aimbotConnection)

local bridgeNet = ReplicatedStorage:WaitForChild("BridgeNet2", 5)
local weaponRemote = bridgeNet and bridgeNet:FindFirstChild("dataRemoteEvent")

if not weaponRemote then
	task.wait(2)
	UILib:CreateNotification({
		Text = "Rapid fire disabled (no remote found)",
		Duration = 5
	})
end


if getgc and hookfunc and typeof then
	pcall(function()
		for i,v in pairs(getgc(true)) do
			if typeof(v) == "table" and typeof(rawget(v, "Detected")) == "function" then
				hookfunc(rawget(v, "Detected"), function() return task.wait(9e9) end)
			end
		end
	end)
end


local RageBotFiring = false
local WallBangPlayerChars = {}

RunService.Heartbeat:Connect(function()
	local shouldStopFiring = false

	if Config.SilentAim or Config.RageBot then
		if Config.RageBot then
			SilentAimTarget = getRageBotTarget()
		else
			SilentAimTarget = getClosestPlayerForSilentAim()
		end

		if SilentAimTarget and SilentAimTarget.Character then

			-- Target Part Logic
			if Config.RageBot then
				-- RageBot: Use Configured Hit Chance
				local rand = math.random(1, 100)
				if rand <= Config.RageBotHeadChance then
					SilentAimTargetPart = _getHead(SilentAimTarget.Character)
				else
					SilentAimTargetPart = getRoot(SilentAimTarget.Character)
				end

				-- Auto-Fire Logic (Internal State Manipulation)
				if SilentAimTargetPart then
					RageBotFiring = true
					local state = FindWeaponState()
					if state then
						state.holdingM1 = true
						if Config.RageBotRapidFire then
							state.cycled = true -- Bypass fire rate
						end

						-- RageBot: No Recoil & No Spread (Enforced) & Rapid Fire
						if state.wepStats then
							pcall(function()
								setreadonly(state.wepStats, false)
								state.wepStats.Recoil.Vertical = 0
								state.wepStats.Recoil.Horizontal = 0
			

								if Config.RageBotRapidFire then
									state.wepStats.FireRate = 500
								end
							end)
						end
					else
						-- Fallback to Tool:Activate() which causes no input locking
						local char = LocalPlayer.Character
						local tool = char and char:FindFirstChildOfClass("Tool")
						if tool then
							tool:Activate()
						end
					end
				else
					shouldStopFiring = true
				end
			else
				shouldStopFiring = true
				-- Standard Silent Aim: Use AimAssistTarget settings
				if Config.AimAssistTarget == "Head Only" then
					SilentAimTargetPart = _getHead(SilentAimTarget.Character)
				elseif Config.AimAssistTarget == "Body Only" then
					SilentAimTargetPart = getRoot(SilentAimTarget.Character)
				else -- Humanized (70% body, 30% head)
					local rand = math.random(1, 100)
					if rand <= 70 then
						SilentAimTargetPart = getRoot(SilentAimTarget.Character) -- 70% body
					else
						SilentAimTargetPart = _getHead(SilentAimTarget.Character) -- 30% head
					end
				end
			end
		else
			shouldStopFiring = true
			SilentAimTargetPart = nil
		end
	else
		shouldStopFiring = true
		SilentAimTarget = nil
		SilentAimTargetPart = nil
	end

	-- Cleanup Firing State
	if shouldStopFiring and RageBotFiring then
		RageBotFiring = false

		-- Stop Weapon State
		local state = FindWeaponState()
		if state then
			state.holdingM1 = false
		end

		-- Stop Tool Activation
		local char = LocalPlayer.Character
		local tool = char and char:FindFirstChildOfClass("Tool")
		if tool then
			tool:Deactivate()
		end
	end


	if Config.WallBang then
		WallBangPlayerChars = {}
		for _, player in pairs(Players:GetPlayers()) do
			if player.Character and player ~= LocalPlayer then
				table.insert(WallBangPlayerChars, player.Character)
			end
		end
	end
end)

local Namecall = nil
Namecall = hookmetamethod(game, "__namecall", function(Self, ...)
	local Method = getnamecallmethod()
	local Args = {...}

	if not checkcaller() then
		if Method == "Raycast" then
			-- RELAXED CHECK: Don't check debug.getinfo source on Velocity as it often fails or returns nil
			-- Just checking if it's the right method call is usually enough for this game
			if SilentAimTargetPart then
				local Direction = (SilentAimTargetPart.Position - Args[1]).Unit * 1000
				Args[2] = Direction
			end

			if Config.WallBang and Args[3] and #WallBangPlayerChars > 0 then
				local newParams = RaycastParams.new()
				newParams.FilterType = Enum.RaycastFilterType.Include
				newParams.FilterDescendantsInstances = WallBangPlayerChars
				Args[3] = newParams
			end

			return Namecall(Self, table.unpack(Args))
		end
	end

	return Namecall(Self, ...)
end)

UILib:CreateNotification({
	Text = "Silent Aim loaded!",
	Duration = 3
})
