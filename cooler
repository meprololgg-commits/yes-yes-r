		Config.Aimbot = value
	end,
	SubToggles = {
		{
			Label = "Wall Check",
			Default = false,
			Callback = function(value)
				Config.WallCheck = value
			end
		},
		{
			Label = "Ignore Ally",
			Default = false,
			Callback = function(value)
				Config.IgnoreAlly = value
			end
		},
		{
			Label = "Show FOV Circle",
			Default = false,
			Callback = function(value)
				Config.ShowAimbotFOV = value
			end
		}
	}
})

