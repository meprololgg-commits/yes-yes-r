		if value then 
			Config.ShowSilentAimFOV = true 
			if Toggles.ShowSilentAimFOV then Toggles.ShowSilentAimFOV:SetValue(true) end
		end
	end,
	SubToggles = {
		{
			Label = "Rapid Fire",
			Default = false,
			Callback = function(value)
				Config.RageBotRapidFire = value
			end
		},
		{
			Label = "Ignore Ally",
			Default = true,
			Callback = function(value)
				Config.IgnoreAlly = value
			end
		},
		{
			Label = "One Tap",
			Default = false,
			Callback = function(value)
				Config.RageBotOneTap = value
			end
		}
	}
})

