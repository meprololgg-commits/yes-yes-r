	Text = "Aimbot Smoothness",
	Min = 1,
	Max = 20,
	Default = 5,
	Callback = function(value)
		Config.AimbotSmooth = value
	end
})

Toggles.SilentAim = UILib:CreateCollapsibleToggle(CombatPanel, {
	Label = "Silent Aim",
	Default = false,
	Callback = function(value)
		Config.SilentAim = value
		if value then Config.ShowSilentAimFOV = true end -- Auto-enable FOV
	end,
	SubToggles = {
		{
			Label = "Wall Check",
			Default = false,
			Callback = function(value)
				Config.WallCheck = value
			end
		},
		{
			Label = "Ignore Ally",
			Default = false,
			Callback = function(value)
				Config.IgnoreAlly = value
			end
		},
		{   
			Label = "Show FOV Circle",
			Default = false,
			Callback = function(value)
				Config.ShowSilentAimFOV = value
			end
		},
		{
			Label = "Wall Bang (Thin Walls)",
			Default = false,
			Callback = function(value)
				Config.WallBang = value
			end
		}
	}
})

-- Manually register sub-toggles for sync (Order matches SubToggles list)
if Toggles.SilentAim and Toggles.SilentAim.SubToggles then
	Toggles.WallCheck = Toggles.SilentAim.SubToggles[1]
	Toggles.IgnoreAlly = Toggles.SilentAim.SubToggles[2]
	Toggles.ShowSilentAimFOV = Toggles.SilentAim.SubToggles[3]
	Toggles.WallBang = Toggles.SilentAim.SubToggles[4]
end

