-- ========================================
-- CONFIG PANEL
-- ========================================
local ConfigPanel = UILib:CreatePanel(Window, {
	Name = "Config",
	DisplayName = "Config"
})

-- Track selected config from dropdown
local SelectedDropdownConfig = "default"
local ConfigNameInput = ""

-- Config name input using UILib
local configNameTextInput = UILib:CreateTextInput(ConfigPanel, {
	Placeholder = "Enter config name...",
	Default = "",
	Callback = function(text, enterPressed)
		ConfigNameInput = text
	end
})

-- Config dropdown using UILib
local configDropdown = UILib:CreateDropdown(ConfigPanel, {
	Label = "Select Config",
	Options = GetConfigList(),
	EnableSearch = false,
	Callback = function(selected)
		SelectedDropdownConfig = selected
	end
})

-- Refresh dropdown options function
local function RefreshConfigDropdown()
	local configs = GetConfigList()
	if #configs == 0 then
		configs = {"default"}
	end
	if configDropdown and configDropdown.UpdateOptions then
		configDropdown.UpdateOptions(configs)
	end
end

-- Initial refresh
task.spawn(function()
	task.wait(0.5)
	RefreshConfigDropdown()
end)

UILib:CreateButton(ConfigPanel, {
	Text = "Save Config",
	Callback = function()
		local name = ConfigNameInput
		if name == "" then name = "default" end
		name = name:gsub("[^%w_%-]", "")
		if name == "" then name = "config" end

		local success, err = SaveConfigAs(name)
		if success then
			UILib:CreateNotification({
				Text = "Config '" .. name .. "' saved!",
				Duration = 2
			})
		else
			UILib:CreateNotification({
				Text = "Failed: " .. tostring(err),
				Duration = 5
			})
		end
	end
})

UILib:CreateButton(ConfigPanel, {
	Text = "Load Selected",
	Callback = function()
		local name = configDropdown and configDropdown.GetValue and configDropdown.GetValue() or SelectedDropdownConfig
		if not name or name == "" then name = "default" end

		local success, err = LoadConfigByName(name)
		if success then
			UILib:CreateNotification({
				Text = "Loaded '" .. name .. "'!",
				Duration = 3
			})
		else
			UILib:CreateNotification({
				Text = "Load Fail: " .. tostring(err),
				Duration = 3
			})
		end
	end
})

UILib:CreateButton(ConfigPanel, {
	Text = "Delete Selected",
	Callback = function()
		local name = configDropdown and configDropdown.GetValue and configDropdown.GetValue() or SelectedDropdownConfig
		if not name or name == "" or name == "default" then
			UILib:CreateNotification({
				Text = "Cannot delete default config",
				Duration = 2
			})
			return
		end

		local success = DeleteConfig(name)
		if success then
			SelectedDropdownConfig = "default"
			if configDropdown and configDropdown.SetValue then
				configDropdown.SetValue("default")
			end
			RefreshConfigDropdown()
			UILib:CreateNotification({
				Text = "Config '" .. name .. "' deleted",
				Duration = 2
			})
		else
			UILib:CreateNotification({
				Text = "Delete failed",
				Duration = 3
			})
		end
	end
})

UILib:CreateButton(ConfigPanel, {
	Text = "Refresh List",
	Callback = function()
		RefreshConfigDropdown()
		local configs = GetConfigList()
		if #configs == 0 then
			UILib:CreateNotification({
				Text = "No saved configs",
				Duration = 2
			})
		else
			UILib:CreateNotification({
				Text = #configs .. " configs found",
				Duration = 2
			})
		end
	end
})

-- NEW: Config Share (Export/Import)
UILib:CreateButton(ConfigPanel, {
	Text = "Export Config to Clipboard",
	Callback = function()
		pcall(function()
			local HttpService = game:GetService("HttpService")
			local json = HttpService:JSONEncode(Config)
			if setclipboard then
				setclipboard(json)
				UILib:CreateNotification({Text = "Config copied to clipboard!", Duration = 2})
			else
				UILib:CreateNotification({Text = "Clipboard not supported!", Duration = 2})
			end
		end)
	end
})

local configImportString = ""
UILib:CreateTextInput(ConfigPanel, {
	Label = "Import Config Data",
	Placeholder = "Paste JSON here...",
	Callback = function(text)
		configImportString = text
	end
})

UILib:CreateButton(ConfigPanel, {
	Text = "Import Config",
	Callback = function()
		if configImportString == "" then
			UILib:CreateNotification({Text = "Paste config data first!", Duration = 2})
			return 
		end

		local success, newConfig = pcall(function() 
			return game:GetService("HttpService"):JSONDecode(configImportString) 
		end)

		if success and type(newConfig) == "table" then
			-- Merge
			for k, v in pairs(newConfig) do
				if Config[k] ~= nil then 
					Config[k] = v 
				end
			end

			-- Force Refresh UI & Visuals (Using existing SyncToggles function)
			pcall(SyncToggles)

			UILib:CreateNotification({Text = "Config Imported Successfully!", Duration = 3})
			-- Optionally clear input? UI lib might not support clearing input field easily
		else
			UILib:CreateNotification({Text = "Invalid Config Data!", Duration = 3})
		end
	end
})

-- Conflicting toggle logic removed
-- Removed InputEnded as we use toggle now
local mainLoop = RunService.RenderStepped:Connect(function()
	updateAimbot()
	updatePlayerESP()
	updateItemESP()
end)
table.insert(_G.RetroBreach.Connections, mainLoop)
local playerLoop = RunService.Heartbeat:Connect(function()
	if LocalPlayer.Character then
		local humanoid = _getHumanoid(LocalPlayer.Character)
		if humanoid then
			if Config.WalkSpeed ~= 16 then
				humanoid.WalkSpeed = Config.WalkSpeed
			end
			if Config.JumpPower ~= 50 then
				humanoid.JumpPower = Config.JumpPower
			end
		end

	end
end)

-- NoClip Physics Loop (Moved to Stepped for proper collision override)
local noclipLoop = RunService.Stepped:Connect(function()
	if Config.NoClip then
		if LocalPlayer.Character then
			for _, part in ipairs(LocalPlayer.Character:GetDescendants()) do
				if part:IsA("BasePart") and part.CanCollide then
					part.CanCollide = false
				end
			end
		end
	end
end)
table.insert(_G.RetroBreach.Connections, noclipLoop)
table.insert(_G.RetroBreach.Connections, playerLoop)
Players.PlayerAdded:Connect(function(player)
	if player.Character and Config.PlayerESP then
		player.Character:WaitForChild("HumanoidRootPart", 5)
		task.wait(0.2)
		createPlayerESP(player.Character)
	end
	player.CharacterAdded:Connect(function(character)
		if Config.PlayerESP then
			character:WaitForChild("HumanoidRootPart", 5)
			task.wait(0.2)
			createPlayerESP(character)
		end
	end)
end)
Players.PlayerRemoving:Connect(function(player)
	if player and player.Character and _G.RetroBreach.ESPHighlights[player.Character] then
		local esp = _G.RetroBreach.ESPHighlights[player.Character]
		if esp.highlight then pcall(function() esp.highlight:Destroy() end) end
		if esp.teamText then pcall(function() esp.teamText:Remove() end) end
		if esp.nameText then pcall(function() esp.nameText:Remove() end) end
		_G.RetroBreach.ESPHighlights[player.Character] = nil
	end
end)
for _, player in ipairs(Players:GetPlayers()) do
	player.CharacterAdded:Connect(function(character)
		if Config.PlayerESP then
			character:WaitForChild("HumanoidRootPart", 5)
			task.wait(0.2)
			createPlayerESP(character)
		end
	end)
end

if UILib.CreateNotification then
	UILib:CreateNotification({
		Text = "Retro Breach Definitive Loaded!",
		Duration = 5,
		Color = UILib.Colors and UILib.Colors.SUCCESS or Color3.fromRGB(0, 255, 0)
	})
end
task.spawn(function()
	task.wait(1)
	local itemsFolder = Workspace:FindFirstChild("ItemSpawns")
	if itemsFolder then
		local itemNames = {}
		for _, item in ipairs(itemsFolder:GetChildren()) do
			if not table.find(itemNames, item.Name) then
				table.insert(itemNames, item.Name)
			end
		end
		table.sort(itemNames)
	else
	end
end)

local _ReplicatedStorage = game:GetService("ReplicatedStorage")
local _pcall = pcall
local _task_wait = task.wait

task.spawn(function()
	while true do
		local success, err = _pcall(function()
			if not Config.InfiniteStamina then 
				_task_wait(0.5)
				return 
			end

			-- SAFETY CHECKS (Prevent crash on death/round reset)
			local char = LocalPlayer.Character
			if not char or not char.Parent then 
				_task_wait(1)
				return 
			end

			-- Check for RootPart (ensures character is physically present)
			if not char:FindFirstChild("HumanoidRootPart") then
				_task_wait(1)
				return
			end

			local humanoid = char:FindFirstChildOfClass("Humanoid")
			if not humanoid or humanoid.Health <= 0 then
				_task_wait(1)
				return
			end

			-- Method 1: Try to find and set stamina value directly (mobile-compatible)
			-- WRAPPED IN PCALL for extra safety on mobile
			pcall(function()
				if humanoid then
					-- Look for stamina attribute or value
					local stamina = humanoid:FindFirstChild("Stamina") or 
						char:FindFirstChild("Stamina")

					if stamina and (stamina:IsA("NumberValue") or stamina:IsA("IntValue")) then
						stamina.Value = 100
					elseif humanoid:GetAttribute("Stamina") ~= nil then
						humanoid:SetAttribute("Stamina", 100)
					end
				end
			end)

			-- Removed Method 2 (firesignal) as it can cause crashes on some mobile executors
		end)

		if not success then
			_task_wait(1)
		end

		_task_wait(0.2)  -- Update rate
	end
end)

-- ==========================================
-- BLATANT FLY LOGIC (RenderStepped)
local function getCamera()
	return workspace.CurrentCamera
end

-- Blatant Fly Removed


-- ==========================================
-- VELOCITY SPOOF BYPASS (Anti-Kick)
-- ==========================================
task.spawn(function()
	local RunService = game:GetService("RunService")

	-- Helper to find closest room for better rescue
	local function getClosestRoom()
		local char = LocalPlayer.Character
		if not char then return "Entrance Zone" end
		local root = char:FindFirstChild("HumanoidRootPart")
		if not root then return "Entrance Zone" end

		local closestName = "Entrance Zone"
		local minDst = math.huge

		-- RoomLocations must be defined globally or in scope
		if RoomLocations then
			for name, cf in pairs(RoomLocations) do
				local dst = (root.Position - cf.Position).Magnitude
				if dst < minDst then
					minDst = dst
					closestName = name
				end
			end
		end
		return closestName
	end

	local fallTime = 0
	local performingRescue = false
	_G.RetroBreach.SafeDropping = false -- Global flag for safe drop

	-- Using Heartbeat to override physics after they step
	RunService.Heartbeat:Connect(function(dt)
		-- Auto-Spoof when flying, rescuing, or SAFE DROPPING
		local shouldSpoof = (Config.RageFly and flying) or performingRescue or _G.RetroBreach.SafeDropping

		-- SAFE DROP LOGIC (Manual Descent)
		if _G.RetroBreach.SafeDropping then
			local char = LocalPlayer.Character
			if char and char:FindFirstChild("HumanoidRootPart") then
				local hrp = char.HumanoidRootPart

				-- Raycast down to find ground
				local rayParams = RaycastParams.new()
				rayParams.FilterType = Enum.RaycastFilterType.Exclude
				rayParams.FilterDescendantsInstances = {char}

				local descentSpeed = 60 * dt -- 60 studs/sec
				local ray = workspace:Raycast(hrp.Position, Vector3.new(0, -descentSpeed - 3, 0), rayParams)

				if ray then
					-- Ground found close by, snap to it and stop
					hrp.CFrame = CFrame.new(ray.Position + Vector3.new(0, 3, 0))
					_G.RetroBreach.SafeDropping = false
					UILib:CreateNotification({Text = "Landed Safely", Duration = 2})
					shouldSpoof = false
				else
					-- Continue moving down manually
					hrp.CFrame = hrp.CFrame - Vector3.new(0, descentSpeed, 0)
				end
			else
				_G.RetroBreach.SafeDropping = false
			end
		end

		-- Void Check and Rescue
		local char = LocalPlayer.Character
		if char then
			local hrp = char:FindFirstChild("HumanoidRootPart")
			local humanoid = char:FindFirstChild("Humanoid")

			if hrp and humanoid then
				-- VOID RESCUE (Smart Raycast Detection)
				-- IGNORE IF FLYING (User Request)
				local isFalling = humanoid:GetState() == Enum.HumanoidStateType.Freefall
				local isBelowMap = hrp.Position.Y < -50
				local isOverVoid = false

				if flying then
					-- Reset timers if flying so it doesn't trigger immediately upon landing
					fallTime = 0
				elseif isFalling then
					-- Cast a ray downwards to check for ground
					local rayParams = RaycastParams.new()
					rayParams.FilterType = Enum.RaycastFilterType.Exclude
					rayParams.FilterDescendantsInstances = {char}

					local rayResult = workspace:Raycast(hrp.Position, Vector3.new(0, -200, 0), rayParams)
					if not rayResult then
						isOverVoid = true
					end
				end

				-- Trigger Condition:
				-- 1. Below Map (Deep Void)
				-- 2. Falling over Void (Immediate detection)
				-- 3. NOT FLYING
				if not flying and (isBelowMap or (isFalling and isOverVoid)) then
					shouldSpoof = true -- IMMEDIATE SPOOF protection
					fallTime = fallTime + dt
				elseif not flying and isFalling and hrp.AssemblyLinearVelocity.Y < -50 then
					-- Fallback: Falling very fast but raycast hit something (maybe glitching through floor?)
					fallTime = fallTime + dt
				else
					fallTime = 0
				end

				if fallTime > 0.5 then
					shouldSpoof = true -- Force spoof to stop fall
					performingRescue = true -- Persist spoofing

					-- DISABLE NOCLIP & RESTORE COLLISION
					if Config.NoClip then
						Config.NoClip = false
						for _, part in ipairs(char:GetDescendants()) do
							if part:IsA("BasePart") then
								part.CanCollide = true 
							end
						end
					end
				end

				if shouldSpoof then
					-- Reset velocity to 0
					hrp.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
					hrp.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
				end
			end
		end
	end)
end)

-- 
-- RAGE FLY SYSTEM (Press V to toggle)
-- System initializes immediately but checks Config.RageFly before activating
-- -------------------------------------------------------
task.spawn(function()

	-- ADVANCED FLY BYPASS
	-- - Periodic ground touches every 1.5s to reset anti-cheat
	-- - Variable speed (average â†’ fast â†’ average) to avoid detection
	-- Controls: V = Fly, E = Noclip, WASD + Space/Shift

	local player = game.Players.LocalPlayer
	local RunService = game:GetService("RunService")
	local UserInputService = game:GetService("UserInputService")

	-- Configuration
	local speedMult = UserInputService.TouchEnabled and 2 or 1
	local BASE_SPEED = 25 * speedMult  -- Slower base speed for better control
	local FAST_SPEED = 40 * speedMult  -- Slower fast speed
	local GROUND_TOUCH_INTERVAL = 2.5  -- Spawn reset every 2.5 seconds (only while shooting)
	local SPEED_CYCLE_TIME = 0.5  -- Cycle speed every 0.5 seconds

	-- State
	-- Using upvalues declared at top
	flying = false
	noclipping = false
	local flyConnection = nil
	local noclipConnection = nil
	local voidBypass = nil
	local lastGroundTouch = 0
	local lastSpeedChange = 0
	local currentSpeed = BASE_SPEED
	local speedPhase = 1  -- 1=average, 2=fast, 3=average
	local isShooting = false
	local mouse1Active = false
	local activeTouches = {}
	local lookTouch = nil
	local lastLookPos = nil
	local bodyAtSpawn = false
	local spawnCFrame = nil
	local mouseConnection = nil
	local camRotX = 0
	local camRotY = 0
	local shootingStartTime = 0  -- Track when shooting started for rage mode

	-- Key states (Upvalue)
	-- keys initialized at top

	-- Get character
	local function getChar()
		return player.Character
	end

	local function getRoot()
		local char = getChar()
		return char and (char:FindFirstChild("HumanoidRootPart") or char:FindFirstChild("Torso"))
	end

	local function getHumanoid()
		local char = getChar()
		return char and char:FindFirstChildOfClass("Humanoid")
	end

	-- Find any nearby part to touch (for anti-cheat reset)
	local function findNearbyPart()
		local hrp = getRoot()
		if not hrp then return nil end

		local rayParams = RaycastParams.new()
		rayParams.FilterType = Enum.RaycastFilterType.Blacklist
		rayParams.FilterDescendantsInstances = {getChar()}

		-- Try different directions to find ANY nearby part (increased to 1000 studs)
		local directions = {
			Vector3.new(0, -1000, 0),   -- Below
			Vector3.new(0, 1000, 0),    -- Above
			Vector3.new(1000, 0, 0),    -- Right
			Vector3.new(-1000, 0, 0),   -- Left
			Vector3.new(0, 0, 1000),    -- Forward
			Vector3.new(0, 0, -1000),   -- Backward
		}

		for _, direction in ipairs(directions) do
			local ray = workspace:Raycast(hrp.Position, direction, rayParams)
			if ray and ray.Instance then
				return ray.Position, ray.Instance
			end
		end

		-- Fallback: Find ANY part in workspace
		for _, obj in pairs(workspace:GetDescendants()) do
			if obj:IsA("BasePart") and obj.Parent ~= getChar() and obj.Name ~= "HumanoidRootPart" then
				return obj.Position, obj
			end
		end

		return nil, nil
	end

	-- Start void bypass
	local function startVoidBypass()
		if voidBypass then return end

		voidBypass = RunService.Heartbeat:Connect(function()
			local hrp = getRoot()
			if hrp and hrp.Parent then
				local currentPos = hrp.Position
				if currentPos.Y < -50 then
					hrp.CFrame = CFrame.new(currentPos.X, -40, currentPos.Z)
					hrp.Velocity = Vector3.new(0, 0, 0)
				end
			end
		end)
	end

	-- Start noclip
	startNoclip = function()
		if noclipping then return end
		noclipping = true


		noclipConnection = RunService.Stepped:Connect(function()
			if not noclipping then return end

			local char = getChar()
			if not char then return end

			for _, part in pairs(char:GetDescendants()) do
				if part:IsA("BasePart") then
					part.CanCollide = false
				end
			end
		end)
	end

	-- Stop noclip
	stopNoclip = function()
		if not noclipping then return end
		noclipping = false


		if noclipConnection then
			noclipConnection:Disconnect()
			noclipConnection = nil
		end

		local char = getChar()
		if char then
			for _, part in pairs(char:GetDescendants()) do
				if part:IsA("BasePart") then
					-- Precise restoration based on R6 collision dump (Head/Torso = True, Rest = False)
					if part.Name == "Head" or part.Name == "Torso" then
						part.CanCollide = true
					else
						part.CanCollide = false
					end
				end
			end
		end
	end

	-- Check if player is above ground
	local function isAboveGround()
		local hrp = getRoot()
		if not hrp then return false end

		local rayParams = RaycastParams.new()
		rayParams.FilterType = Enum.RaycastFilterType.Blacklist
		rayParams.FilterDescendantsInstances = {getChar()}

		-- Raycast straight down (500 studs)
		local ray = workspace:Raycast(hrp.Position, Vector3.new(0, -500, 0), rayParams)

		return ray ~= nil
	end

	-- Start flying
	local originalPosition = nil  -- Store original position before flying
	local safeSpotEndTime = 0 -- Timer for non-blocking safe spot logic

	startFly = function()
		if flying then return end
		if not Config.RageFly then return end  -- Only fly if toggle is enabled
		flying = true

		if _G.RetroBreach.MobileControls then
			_G.RetroBreach.MobileControls.Enabled = true
		end

		local hrp = getRoot()
		if not hrp then 
			return 
		end

		-- SAVE ORIGINAL POSITION before teleporting anywhere
		originalPosition = hrp.CFrame

		-- Set Safe Spot (Fixed Coordinate)
		local safeSpotPos = Vector3.new(1485.03, 585.50, 19.55)
		spawnCFrame = CFrame.new(safeSpotPos)

		-- Reset state to ensure fresh start
		bodyAtSpawn = false
		isShooting = false

		lastGroundTouch = tick()
		lastSpeedChange = tick()
		safeSpotEndTime = 0 -- Initialize Safe Spot timer

		-- Reset camera rotation
		camRotX = 0
		camRotY = 0

		-- Force third person (save original zoom)
		local cam = workspace.CurrentCamera
		local humanoid = getHumanoid()
		if humanoid then
			humanoid.CameraOffset = Vector3.new(0, 0, 0)
		end
		cam.CameraSubject = getHumanoid()

		-- Zoom out to third person
		player.CameraMaxZoomDistance = 15
		player.CameraMinZoomDistance = 10

		-- Wait for camera to actually zoom out
		task.wait(0.3)

		-- NOW set flying to true after camera is ready
		flying = true

		-- Unlock mouse for camera control
		UserInputService.MouseBehavior = Enum.MouseBehavior.LockCenter
		UserInputService.MouseIconEnabled = false

		-- Get mouse for delta reading
		local mouse = player:GetMouse()

		flyConnection = RunService.Heartbeat:Connect(function()
			if not flying then return end

			local hrp = getRoot()
			if not hrp then
				stopFly()
				return
			end

			local currentTime = tick()
			local cam = workspace.CurrentCamera

			-- Update camera rotation
			-- PC: Use Mouse Delta
			if not UserInputService.TouchEnabled then
				UserInputService.MouseBehavior = Enum.MouseBehavior.LockCenter
				local mouseDelta = UserInputService:GetMouseDelta()
				camRotY = camRotY - mouseDelta.X * 0.004
				camRotX = math.clamp(camRotX - mouseDelta.Y * 0.004, -math.pi/2 + 0.1, math.pi/2 - 0.1)
			end
			-- Mobile: Rotation handled by InputChanged event (Custom Look Touch) to prevent Joystick interference

			-- Variable speed cycling (keep this for less predictable movement)
			if currentTime - lastSpeedChange >= SPEED_CYCLE_TIME then
				speedPhase = speedPhase + 1
				if speedPhase > 3 then
					speedPhase = 1
				end

				if speedPhase == 1 then
					currentSpeed = BASE_SPEED
				elseif speedPhase == 2 then
					currentSpeed = FAST_SPEED
				else
					currentSpeed = BASE_SPEED
				end

				lastSpeedChange = currentTime
			end

			-- Calculate movement direction
			local direction = Vector3.new(0, 0, 0)
			local h = getHumanoid()

			-- PC Keys
			if keys.w then direction = direction + (cam.CFrame.LookVector * currentSpeed) end
			if keys.s then direction = direction - (cam.CFrame.LookVector * currentSpeed) end
			if keys.a then direction = direction - (cam.CFrame.RightVector * currentSpeed) end
			if keys.d then direction = direction + (cam.CFrame.RightVector * currentSpeed) end
			if keys.space then direction = direction + (Vector3.new(0, 1, 0) * currentSpeed) end
			if keys.shift then direction = direction - (Vector3.new(0, 1, 0) * currentSpeed) end 

			-- Mobile Joystick Support (PlayerModule)
			pcall(function()
				local moveVector = require(player.PlayerScripts.PlayerModule):GetControls():GetMoveVector()
				if moveVector.Magnitude > 0 then
					direction = direction + (cam.CFrame.LookVector * (moveVector.Z * -1) * currentSpeed)
					direction = direction + (cam.CFrame.RightVector * moveVector.X * currentSpeed)
				end
			end)

			-- Mobile Jump Button Support (Ascend)
			if h and h.Jump then
				direction = direction + (Vector3.new(0, 1, 0) * currentSpeed)
			end 

			-- NEW SYSTEM: Body positioning based on shooting
			if isShooting then
				-- Safe Spot Trigger Logic (Non-blocking)
				if currentTime - lastGroundTouch >= GROUND_TOUCH_INTERVAL then
					local isRageMode = (tick() - shootingStartTime >= 7)
					local safeTime = isRageMode and 3.0 or 0.5

					-- Check for void (extended safe time)
					local rayParams = RaycastParams.new()
					rayParams.FilterType = Enum.RaycastFilterType.Blacklist
					rayParams.FilterDescendantsInstances = {getChar()}
					local ray = workspace:Raycast(cam.CFrame.Position, Vector3.new(0, -500, 0), rayParams)
					if not ray then safeTime = 2.0 end -- Void penalty

					safeSpotEndTime = currentTime + safeTime
					lastGroundTouch = currentTime
				end

				-- Check if we are currently in "Safe Spot Mode" (Anti-Cheat Reset)
				if currentTime < safeSpotEndTime then
					-- SAFE SPOT ACTIVE: Force body to safe spot, allow camera to move freely (Ghost Mode)
					local safeSpotPos = Vector3.new(1072.70, 49.00, 343.60)

					-- Teleport body
					hrp.CFrame = CFrame.new(safeSpotPos)
					hrp.Velocity = Vector3.new(0, 0, 0)
					hrp.RotVelocity = Vector3.new(0, 0, 0)

					-- Camera Logic: Continues to move freely!
					cam.CameraType = Enum.CameraType.Scriptable
					local newCamPos = cam.CFrame.Position + (direction * 0.016)
					cam.CFrame = CFrame.new(newCamPos) * CFrame.fromEulerAnglesYXZ(camRotX, camRotY, 0)

					bodyAtSpawn = false -- Mark that body isn't at normal spawn
				else
					-- NORMAL SHOOTING FLY: Body follows camera
					cam.CameraType = Enum.CameraType.Scriptable

					-- Move camera with rotation
					local newCamPos = cam.CFrame.Position + (direction * 0.016)
					cam.CFrame = CFrame.new(newCamPos) * CFrame.fromEulerAnglesYXZ(camRotX, camRotY, 0)

					-- Teleport body TO camera position
					if UserInputService.TouchEnabled then
						-- Mobile: Fully Aligned
						hrp.CFrame = cam.CFrame
						if player.Character then
							for _, part in pairs(player.Character:GetDescendants()) do
								if part:IsA("BasePart") then
									part.LocalTransparencyModifier = 1
								end
							end
						end
					else
						-- PC: Position Only
						hrp.CFrame = CFrame.new(cam.CFrame.Position)
					end

					hrp.Velocity = Vector3.new(0, 0, 0)
					hrp.RotVelocity = Vector3.new(0, 0, 0)

					if hrp.AssemblyLinearVelocity then
						hrp.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
					end
					if hrp.AssemblyAngularVelocity then
						hrp.AssemblyAngularVelocity = Vector3.new(0, 0, 0)
					end

					bodyAtSpawn = false
				end
			else
				-- When not shooting, keep body at spawn and move camera freely
				if spawnCFrame then
					hrp.CFrame = spawnCFrame
					hrp.Velocity = Vector3.new(0, 0, 0)
					if hrp.AssemblyLinearVelocity then
						hrp.AssemblyLinearVelocity = Vector3.new(0, 0, 0)
					end
					bodyAtSpawn = true
				end

				-- Camera moves freely (independent of body)
				cam.CameraType = Enum.CameraType.Scriptable

				-- Move camera with rotation (YXZ order for proper FPS camera)
				local newCamPos = cam.CFrame.Position + (direction * 0.016)
				cam.CFrame = CFrame.new(newCamPos) * CFrame.fromEulerAnglesYXZ(camRotX, camRotY, 0)
			end
		end)
	end

	-- Stop flying
	stopFly = function()
		if not flying then return end
		flying = false

		if _G.RetroBreach.MobileControls then
			_G.RetroBreach.MobileControls.Enabled = false
		end


		if flyConnection then
			flyConnection:Disconnect()
			flyConnection = nil
		end

		if mouseConnection then
			mouseConnection:Disconnect()
			mouseConnection = nil
		end

		local hrp = getRoot()
		if hrp then
			hrp.Velocity = Vector3.new(0, 0, 0)

			-- RESTORE ORIGINAL POSITION
			if originalPosition then
				hrp.CFrame = originalPosition
			end

			-- Restore body visibility (Reset FPS Mode)
			for _, part in pairs(player.Character:GetDescendants()) do
				if part:IsA("BasePart") then
					part.LocalTransparencyModifier = 0
				end
			end
		end

		-- Restore camera
		local cam = workspace.CurrentCamera
		cam.CameraType = Enum.CameraType.Custom
		cam.CameraSubject = getHumanoid()

		-- Restore camera zoom
		player.CameraMaxZoomDistance = 128
		player.CameraMinZoomDistance = 0.5

		-- Restore mouse behavior
		UserInputService.MouseBehavior = Enum.MouseBehavior.Default
		UserInputService.MouseIconEnabled = true

		-- Hide mobile controls
		if _G.RetroBreach.MobileControls then
			_G.RetroBreach.MobileControls.Enabled = false
		end
	end

	-- Mobile Controls GUI
	local function createMobileControls()
		if not isMobile then return end
		-- Removed TouchEnabled check

		local screenGui = Instance.new("ScreenGui")
		screenGui.Name = "RageFlyControls"
		screenGui.ResetOnSpawn = false
		screenGui.Parent = game.CoreGui
		_G.RetroBreach.MobileControls = screenGui
		screenGui.Enabled = false
		table.insert(_G.RetroBreach.Guis, screenGui)

		local function createBtn(text, pos, callback)
			local btn = Instance.new("TextButton")
			btn.Size = UDim2.fromOffset(60, 60)
			btn.Position = pos
			btn.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
			btn.BackgroundTransparency = 0.5
			btn.Text = text
			btn.TextColor3 = Color3.new(1, 1, 1)
			btn.TextSize = 20
			btn.Font = Enum.Font.GothamBold
			btn.Parent = screenGui

			Instance.new("UICorner", btn).CornerRadius = UDim.new(1, 0)

			btn.MouseButton1Down:Connect(function() callback(true) end)
			btn.MouseButton1Up:Connect(function() callback(false) end)
			btn.MouseLeave:Connect(function() callback(false) end)
		end

		-- Right side controls (Up/Down)
		createBtn("UP", UDim2.new(1, -150, 0.6, 0), function(p) keys.space = p end)
		createBtn("DN", UDim2.new(1, -150, 0.75, 0), function(p) keys.shift = p end)

		-- Left side controls (Forward/Back) - relying on thumbstick mostly but adding boost
		-- NOTE: Mobile users use thumbstick for WASD, so we mostly need Up/Down (Space/Shift)
	end

	-- Initialize mobile controls (UP/DOWN buttons)
	createMobileControls()

	-- Global toggles and Mobile Controls visibility handled in functions above

	-- Helper to update shooting state
	local function updateShootingState()
		local newState = mouse1Active or (next(activeTouches) ~= nil)

		if newState and not isShooting then
			shootingStartTime = tick()
			lastGroundTouch = tick()
			isShooting = true
		elseif not newState and isShooting then
			shootingStartTime = 0
			bodyAtSpawn = false
			isShooting = false
		end
	end

	-- Mobile Fire Button Listener
	task.spawn(function()
		if not UserInputService.TouchEnabled then return end

		local mobileFireButton = nil
		local startTime = tick()

		-- Try to find button for 10 seconds
		while not mobileFireButton and tick() - startTime < 10 do
			pcall(function()
				mobileFireButton = LocalPlayer.PlayerGui:FindFirstChild("ScreenGui") 
					and LocalPlayer.PlayerGui.ScreenGui:FindFirstChild("MobileButtons") 
					and LocalPlayer.PlayerGui.ScreenGui.MobileButtons:FindFirstChild("FireButton")
			end)
			if not mobileFireButton then task.wait(1) end
		end

		if mobileFireButton then
			mobileFireButton.InputBegan:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
					activeTouches[input] = true
					updateShootingState()
				end
			end)

			mobileFireButton.InputEnded:Connect(function(input)
				if activeTouches[input] then
					activeTouches[input] = nil
					updateShootingState()
				end
			end)
		end
	end)

	-- Key input handling
	UserInputService.InputBegan:Connect(function(input, gameProcessed)
		-- Detect shooting via MouseButton1 OR Mobile Right-Side Touch (Aiming/Firing)
		local stateChanged = false

		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			mouse1Active = true
			stateChanged = true
		elseif input.UserInputType == Enum.UserInputType.Touch then
			-- Mobile Look Logic: Right 60% of screen = Look Zone
			-- This logic ONLY handles camera rotation, not shooting.
			local cam = workspace.CurrentCamera
			if cam and input.Position.X > cam.ViewportSize.X * 0.4 then
				if not lookTouch then
					lookTouch = input
					lastLookPos = input.Position
				end
			end
		end

		if stateChanged then
			if not UserInputService:GetFocusedTextBox() then
				updateShootingState()
			end
		end

		if stateChanged then
			if not UserInputService:GetFocusedTextBox() then
				updateShootingState()
			end
		end

		if gameProcessed then return end

		local key = input.KeyCode.Name:lower()

		if key == "v" then  -- Restored V for PC (Mobile uses GUI button)
			if _G.RetroBreach.ToggleFly then
				_G.RetroBreach.ToggleFly()
			end
		elseif key == "b" then
			-- MODIFIED: Hold to Noclip (Key B)
			if startNoclip then
				startNoclip()
			end
		elseif keys[key] ~= nil then
			keys[key] = true
		elseif key == "space" then
			keys.space = true
		elseif key == "leftshift" or key == "rightshift" then
			keys.shift = true
		elseif key == "c" then
			-- Rage Fly Teleport Body
			if _G.RetroBreach.TeleportToCam then
				_G.RetroBreach.TeleportToCam()
			end
		end
	end)

	-- EXPOSE TELEPORT FUNCTION GLOBALLY (Inside closure to access upvalues)
	_G.RetroBreach.TeleportToCam = function()
		if not flying then 
			UILib:CreateNotification({Text = "Not flying!", Duration = 1})
			return 
		end

		local hrp = getRoot()
		local cam = workspace.CurrentCamera
		local targetCF = cam.CFrame -- Capture target BEFORE disabling fly

		-- 1. Disable Rage Fly
		Config.RageFly = false

		-- Sync UI if possible
		if Toggles.RageFly and Toggles.RageFly.SetState then
			Toggles.RageFly:SetState(false)
		end

		-- Stop fly loop manually if needed (SetState usually handles it via callback)
		if stopFly then stopFly() end

		-- 2. Teleport logic
		task.spawn(function()
			-- Wait a tiny bit for physics to reset from "fly mode"
			task.wait(0.1)

			if hrp then
				-- 3. Teleport Body to captured camera position
				hrp.CFrame = targetCF

				-- Reset velocity to ensure stable landing
				hrp.Velocity = Vector3.new(0,0,0)
				if hrp.AssemblyLinearVelocity then
					hrp.AssemblyLinearVelocity = Vector3.new(0,0,0)
				end
				hrp.RotVelocity = Vector3.new(0,0,0)
				if hrp.AssemblyAngularVelocity then
					hrp.AssemblyAngularVelocity = Vector3.new(0,0,0)
				end

				-- Enable SAFE DROP to prevent fall kick
				_G.RetroBreach.SafeDropping = true

				-- Visual Feedback
				UILib:CreateNotification({
					Text = "Teleported (Dropping Safely...)",
					Duration = 2
				})
			end
		end)
	end

	UserInputService.InputEnded:Connect(function(input)
		local key = input.KeyCode.Name:lower()

		if keys[key] ~= nil then
			keys[key] = false
		elseif key == "space" then
			keys.space = false
		elseif key == "leftshift" or key == "rightshift" then
			keys.shift = false
		elseif key == "b" then
			-- MODIFIED: Release to Stop Noclip
			if stopNoclip then
				stopNoclip()
			end
		end

		-- Stop shooting
		if input.UserInputType == Enum.UserInputType.MouseButton1 then
			mouse1Active = false
			updateShootingState()
		elseif input.UserInputType == Enum.UserInputType.Touch then
			if activeTouches[input] then
				activeTouches[input] = nil
				updateShootingState()
			end

			if input == lookTouch then
				lookTouch = nil
			end
		end
	end)

	UserInputService.InputChanged:Connect(function(input, gameProcessed)
		if input == lookTouch then
			local delta = input.Position - lastLookPos
			camRotY = camRotY - delta.X * 0.005
			camRotX = math.clamp(camRotX - delta.Y * 0.005, -math.pi/2 + 0.1, math.pi/2 - 0.1)
			lastLookPos = input.Position
		end
	end)

	-- Character respawn handling
	player.CharacterAdded:Connect(function()
		task.wait(1)
		if flying then
			stopFly()
		end
		if noclipping then
			stopNoclip()
		end
	end)



end) -- End of Rage Fly task.spawn

-- ==========================================
-- SYNC VISUALS WITH CONFIG
-- ==========================================
-- Forces all features to update immediately on load without needing retoggle
task.spawn(function()
	task.wait(2) -- Wait for everything to load

	-- Sync ESP
	if Config.PlayerESP then updatePlayerESP() end
	if Config.ItemESP then updateItemESP() end
	if Config.Aimbot then updateAimbot() end
end)

