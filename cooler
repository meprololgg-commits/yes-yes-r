local originalCollisionStates = {}
local noclipKeyHeld = false

Toggles.NoClip = UILib:CreateToggle(PlayerPanel, {
	Label = "NoClip (Toggle Or Hold B)",
	Default = false,
	Callback = function(value)
		Config.NoClip = value
		if not value and not noclipKeyHeld and LocalPlayer.Character then
			-- Restore collision
			for part, originalState in pairs(originalCollisionStates) do
				if part and part.Parent then
					part.CanCollide = originalState
				end
			end
			originalCollisionStates = {}
		elseif value and LocalPlayer.Character then
			-- Enable Noclip: Save states first
			for _, part in ipairs(LocalPlayer.Character:GetDescendants()) do
				if part:IsA("BasePart") then
					if originalCollisionStates[part] == nil then
						originalCollisionStates[part] = part.CanCollide
					end
					part.CanCollide = false
				end
			end
		end
	end
})


