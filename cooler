-- ========================================
-- CONFIG SAVE/LOAD SYSTEM (Multi-Config)
-- ========================================
local CONFIG_FOLDER = "RetroBreach_Configs"
local SelectedConfigName = "default"

-- Keys to save (excludes runtime-only settings)
local SaveableKeys = {
	"PlayerESP", "SCPESP", "ItemESP", "StaffNotify", "StaffAutoHop",
	"ESPShowRole", "ESPShowName", "ESPShowHealth", "ESPShowItems",
	"Aimbot", "AimbotFOV", "AimbotSmooth", "ShowAimbotFOV", "AimAssistTarget",
	"TeamCheck", "WallCheck", "IgnoreAlly", "SilentAim", "SilentAimFOV",
	"ShowSilentAimFOV", "CycleShot", "WallBang", "RapidFire", "InfiniteAmmo",
	"NoRecoil", "NoSpread", "DamageMultiplier", "WalkSpeed", "JumpPower",
	"NoClip", "InfiniteStamina", "FullBright", "ItemTeleport", "InstantReload", "RageFly",
	"StreamerMode", "SpoofedName", "PocketESP", "SCP914Spam", "MobileAiming", "AutoRefillAmmo",
	"RageBot", "RageBotOneTap", "RageBotRapidFire", "RageBotHeadChance",

	"WeaponModEnabled", "WeaponFireRate", "WeaponRecoilVertical", 
	"WeaponRecoilHorizontal", "WeaponSpread", "WeaponMuzzleVelocity",
	"WeaponBulletsFired", "WeaponViewPunch", "WeaponBurstAmount",
	"WeaponInitialSpread", "WeaponMaxSpread", "WeaponReloadSpeed",
	"WeaponStartDamageDropoff", "WeaponMaxDamageDropOff"
}

-- Ensure config folder exists
local function EnsureConfigFolder()
	if isfolder and isfolder(CONFIG_FOLDER) then return true end

	if not makefolder then return false, "makefolder not supported" end

	makefolder(CONFIG_FOLDER)

	if isfolder and not isfolder(CONFIG_FOLDER) then
		return false, "Folder creation failed"
	end

	return true
end

-- Get list of saved configs
local function GetConfigList()
	local configs = {}
	pcall(function()
		if not listfiles then return end
		EnsureConfigFolder()
		local files = listfiles(CONFIG_FOLDER)
		for _, filePath in ipairs(files) do
			local fileName = filePath:match("([^/\\]+)$") or filePath
			if fileName:match("%.json$") then
				local configName = fileName:gsub("%.json$", "")
				table.insert(configs, configName)
			end
		end
	end)
	return configs
end

-- Save config with a name
local function SaveConfigAs(configName)
	local success, result = pcall(function()
		if not writefile then return false, "writefile not supported" end

		local folderOk, folderErr = EnsureConfigFolder()
		if not folderOk then return false, "FS Error: " .. tostring(folderErr) end

		local saveData = {}
		for _, key in ipairs(SaveableKeys) do
			if Config[key] ~= nil then
				saveData[key] = Config[key]
			end
		end

		local HttpService = game:GetService("HttpService")
		local json = HttpService:JSONEncode(saveData)
		writefile(CONFIG_FOLDER .. "/" .. configName .. ".json", json)
		return true
	end)
	if not success then return false, result end
	return unpack(type(result) == "table" and result or {result})
end

-- Toggle registry for UI sync
local Toggles = {}
local Sliders = {}
local Dropdowns = {}
local TextInputs = {}

-- Function to sync UI toggles with Config values (called after loading)
local function SyncToggles()
	for key, toggle in pairs(Toggles) do
		if toggle and toggle.SetState and Config[key] ~= nil then
			pcall(function()
				-- Use SetState to trigger the callback and actually enable/disable features
				toggle.SetState(Config[key])
			end)
		end
	end
	for key, slider in pairs(Sliders) do
		if slider and slider.SetValue and Config[key] ~= nil then
			pcall(function()
				slider.SetValue(Config[key])
			end)
		end
	end
	for key, dropdown in pairs(Dropdowns) do
		if dropdown and dropdown.SetValue and Config[key] ~= nil then
			pcall(function()
				dropdown.SetValue(Config[key])
			end)
		end
	end
	for key, input in pairs(TextInputs) do
		if input and input.SetValue and Config[key] ~= nil then
			pcall(function()
				input.SetValue(Config[key])
			end)
		end
	end
end

-- Load config by name
local function LoadConfigByName(configName)
	local success, result = pcall(function()
		if not readfile or not isfile then return false, "readfile/isfile unsupported" end
		local filePath = CONFIG_FOLDER .. "/" .. configName .. ".json"

		-- Use pcall for isfile to avoid permission errors
		local fileExists = false
		pcall(function() fileExists = isfile(filePath) end)

		if not fileExists then return false, "File not found" end

		local HttpService = game:GetService("HttpService")
		local json = readfile(filePath)
		if not json or json == "" then return false, "Empty file" end

		local loadedData = HttpService:JSONDecode(json)

		for key, value in pairs(loadedData) do
			if Config[key] ~= nil then
				Config[key] = value
			end
		end
		SelectedConfigName = configName

		-- Sync UI toggles with loaded config values
		pcall(SyncToggles)

		return true
	end)
	if not success then return false, result end
	return unpack(type(result) == "table" and result or {result})
end

-- Delete config by name
local function DeleteConfig(configName)
	local success = pcall(function()
		if not delfile then return false end
		local filePath = CONFIG_FOLDER .. "/" .. configName .. ".json"
		if isfile and isfile(filePath) then
			delfile(filePath)
			return true
		end
		return false
	end)
	return success
end

-- Auto-load default config on script start
-- DISABLED BY USER REQUEST: Start fresh every time to avoid ghost settings
