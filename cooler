	Label = "Aim Assist Target",
	Options = {"Head Only", "Body Only", "Humanized"},
	Default = "Humanized",
	Callback = function(value)
		Config.AimAssistTarget = value
	end
})

Toggles.AutoRefillAmmo = UILib:CreateToggle(CombatPanel, {
	Label = "Auto Refill Ammo",
	Default = false,
	Callback = function(state)
		Config.AutoRefillAmmo = state
		if state then
			task.spawn(function()
				local isRefilling = false
				while Config.AutoRefillAmmo do
					task.wait(1)
					if not isRefilling then
						local char = LocalPlayer.Character
						if char then
							local tool, cur, res = getEquippedWeaponAmmo(char)
							if tool and cur and res then
								-- Check if empty (0 current AND 0 reserve)
								if (cur.Value <= 0 and res.Value <= 0) then
									isRefilling = true

									UILib:CreateNotification({
										Text = "Ammo Empty! Auto-Refilling...",
										Duration = 2
									})

									-- Determine ammo type (Pass tool object to check Attributes)
									local isPistol = isPistolWeapon(tool)
									local targetType = isPistol and "SecAmmo" or "PrimAmmo"

									-- Execute grab (will handle unequip/reequip automatically)
									grabAmmoFromWorld(targetType, 2)

									isRefilling = false
								end
							end
						end
					end
				end
			end)
		end
	end
})






